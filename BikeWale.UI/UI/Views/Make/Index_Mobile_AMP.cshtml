        Model.AdTags.Ad_320x50 = true;
        Model.AdTags.Ad300x250_Bottom = true;
        Model.AdTags.AdPath = "/1017752/Bikewale_Mobile_Make";
        Model.AdTags.AdId = "1444028878952";
