        string GetGTTargeting(int targetId, string target)
        {
            switch (targetId)
            {
                case 1:
                    return String.Format("googletag.pubads().setTargeting(\"Model\", \"{0}\");", target.RemoveSpecialCharacters());
                case 2:
                    return String.Format("googletag.pubads().setTargeting(\"Make\", \"{0}\");", target.RemoveSpecialCharacters());
                case 3:
                    return String.Format("googletag.pubads().setTargeting(\"CompareBike-D\", {0});", target);
                    
                case 4:
                    return String.Format("googletag.pubads().setTargeting(\"City\", \"{0}\");", target.RemoveSpecialCharacters());
                case 5:
                    return string.Format("googletag.pubads().setTargeting(\"Series\", \"{0}\");", target.RemoveSpecialCharacters());
                case 6:
                    return string.Format("googletag.pubads().setTargeting(\"Tags\", \"{0}\");", target.RemoveSpecialCharacters());
                default:
                    return "";
            }
        }
        }

