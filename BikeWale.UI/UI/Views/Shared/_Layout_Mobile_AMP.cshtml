                    <div class="top-border" [hidden]="searchterm != ''">
                        <div class="search-title">Trending Searches</div>

                        <amp-list id="trendingsuggest-list" layout="responsive" src="@System.Configuration.ConfigurationManager.AppSettings["bwhosturl"]/api/popularbikes/?topCount=5" width="200" height="120" items=".">
                            <template type="amp-mustache">   
                                <amp-img class="search-icon" src="https://imgd.aeplcdn.com/0x0/bw/static/icons/6f6f6f/arrow-trending.svg" width="17" height="12"></amp-img>
                                <span on="tap: AMP.navigateTo(url= '@System.Configuration.ConfigurationManager.AppSettings["bwhosturl"]/m/{{objMake.maskingName}}-bikes/{{objModel.maskingName}}/')" role="link" tabindex="2" class="search__result">{{BikeName}}</span>
                            </template>
                        </amp-list>
                    </div>
                    <div class="top-border" [hidden]="searchterm == ''">
                        <div [hidden]="suggest.suggestionList.length != 0">
                            <span class="text-bold padding10">Oops! No suggestions found</span><br /> <span class="padding10">Search by bike name e.g: Honda Activa</span>
                        </div>
                        <amp-list id="globalsearchsuggest-list" layout="responsive" [src]="'@System.Configuration.ConfigurationManager.AppSettings["bwhosturl"]/api/AutoSuggest/?source=1&noofrecords=5&inputText=' + (searchterm || '')" width="200" height="120" hidden="" items="suggestionList" reset-on-refresh>
                            <template type="amp-mustache">
                                <amp-img class="search-icon" src="https://imgd.aeplcdn.com/0x0/bw/static/icons/6f6f6f/search.svg" width="17" height="16"></amp-img>
                                <span class="search__result " on="tap: AMP.navigateTo(url='@System.Configuration.ConfigurationManager.AppSettings["bwhosturl"]/m/{{payload.makeMaskingName}}-bikes/{{payload.modelMaskingName}}/')" role="link" tabindex="2">{{text}}</span>
                            </template>
                        </amp-list>
                      </div>
                 </div>
            </div>
        </div>
        <div class="blackOut-window"><a id="blackOut-window" href="#" on="tap: global__search.close"></a></div>
    </amp-lightbox>

    @if (Model.BreadcrumbList != null && Model.BreadcrumbList.BreadcrumListItem != null && Model.BreadcrumbList.BreadcrumListItem.Any())
    {
        <section>
            <div class="breadcrumb">
                <span class="breadcrumb-title">You are here:</span>
                <ul>
                    @{
                    var breadcrumItemCount = Model.BreadcrumbList.BreadcrumListItem.Count();
                    short currentBreadCrum = 0;
                    foreach (var item in Model.BreadcrumbList.BreadcrumListItem)
                    {
                        if (!string.IsNullOrEmpty(item.Item.Url) && (breadcrumItemCount - 1) > currentBreadCrum++)
                        {
                            <li>
                                <a class="breadcrumb-link" href="@item.Item.Url" title="@item.Item.Name">
                                    <span class="breadcrumb-link__label" itemprop="name">@item.Item.Name</span>
                                </a>
                            </li>
                        }
                        else
                        {
                            <li>
                                <span class="breadcrumb-link__label">@item.Item.Name</span>
                            </li>
                        }
                    }
                    }
                </ul>
                <div class="clear"></div>
            </div>
            <div class="clear"></div>
        </section>
    }

    @if (Model.AdTags != null && Model.AdTags.Ad_Bot_320x50)
    {
        <div class="margin-bottom10 margin-top15">
            @BWHelpers.RenderAmpAds(320, 50, string.Format("{0}_Bottom_320x50", Model.AdTags.AdPath), @Html.Raw(setTargettingJson))
        </div>        
    }


    @if (Model.AdTags != null && Model.AdTags.Ad300x250_Bottom)
    {
