    <script async custom-template="amp-mustache" src="https://cdn.ampproject.org/v0/amp-mustache-0.2.js"></script>

    @if (Model != null && Model.PageMetaTags != null)
    {
        Model.PageMetaTags.EnableOG = true;

        if (!String.IsNullOrEmpty(Model.PageMetaTags.Title))
        {
            <title itemprop="name">@Html.Raw(Model.PageMetaTags.Title)</title>
        }
        if (!String.IsNullOrEmpty(Model.PageMetaTags.Description))
        {
            <meta itemprop="description" name="description" content="@Model.PageMetaTags.Description" />
        }
        if (!String.IsNullOrEmpty(Model.PageMetaTags.Keywords))
        {
            <meta name="keywords" content="@Model.PageMetaTags.Keywords" />
        }
        if (!String.IsNullOrEmpty(Model.PageMetaTags.CanonicalUrl))
        {
            <link rel="canonical" href="@Model.PageMetaTags.CanonicalUrl" />
        }

    }
    @if (Model.PageMetaTags.EnableOG)
    {
        <meta property="og:title" content="@Html.Raw(Model.PageMetaTags.Title)" />
        <meta property="og:type" content="website" />
        <meta property="og:description" content="@Html.Raw(Model.PageMetaTags.Description)" />
        <meta itemprop="image" property="og:image" content="@(string.IsNullOrEmpty(Model.PageMetaTags.OGImage) ? Bikewale.Utility.BWConfiguration.Instance.BikeWaleLogo : Model.PageMetaTags.OGImage)" />
        if (!String.IsNullOrEmpty(Model.PageMetaTags.CanonicalUrl))
        {
            <meta property="og:url" content="@Model.PageMetaTags.CanonicalUrl" />
        }
    }
    <meta name="viewport" content="width=device-width,minimum-scale=1,initial-scale=1">
    <meta name="amp-google-client-id-api" content="googleanalytics">
