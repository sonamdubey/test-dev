@model Bikewale.Entities.manufacturecampaign.v2.ManufactureCampaignLeadEntity
@using Bikewale.Utility
@if(Model!=null)
{
    <!-- Manufacturer Campaign Ad section starts -->
    try
    {
        string hash = Bikewale.PWA.Utils.PwaCmsHelper.GetSha256Hash(Model.LeadsHtmlDesktop);
        @Html.Raw(MvcHelper.Render(hash, Model, Model.LeadsHtmlDesktop))
    }
    catch (Exception ex)
    {
        Bikewale.Notifications.ErrorClass.LogError(ex, String.Format("ManufacturerCampaign.Desktop(CampaignId : {0})",Model.CampaignId));
    }
     <!-- Manufacturer Campaign Ad section ends -->
}