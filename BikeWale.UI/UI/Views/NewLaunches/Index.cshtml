@model Bikewale.Models.NewLaunchedIndexVM
@if(Model != null) {
{
    Model.Page_ATF_CSS = "<link rel='stylesheet' type='text/css' href='/UI/sass/new-launch/new-launch.css' inline />";
    Model.Page_JS_Path = "/UI/src/new-launch.js";
    Model.AdTags.Ad_970x90Top = true;
    Model.AdTags.Ad_970x90Bottom = true;
    Model.AdTags.AdId = "1517814992282";
    Model.AdTags.AdPath = "/1017752/Bikewale_NewBikeLaunches_";
    Model.IsPageTypeLanding = true;
    Layout = "~/UI/Views/Shared/_Layout.cshtml";
    Model.IsHeaderFix = true;
}

<section>
    <div class="container">
        <div class="grid-12">
			<div class="content-box-shadow">
				<h1 class="content-box-shadow padding-15-20">@Model.Page_H1</h1>
				<div class="padding-15-20 font14 text-light-grey collapsible-content">
					<p class="main-content">India is the world's largest two wheeler market. The sheer number of bikes and the need to keep up with or surpass the competition means that newly launched bikes are a regular feature. To make it easier for you to keep up with the latest bikes and launches in the Indian market, we've put all of them in a single place.</p>
				</div>
			</div>
        </div>
        <div class="clear"></div>
    </div>
</section>
<section>
    <div id="newlaunched-bikes" data-city="@Model.CityId" class="container section-bottom-margin">
        <div class="grid-12">
            <h2 class="section-heading">New bike launches in India</h2>
            @Html.Partial("~/UI/Views/NewLaunches/_NewLaunchedBikes.cshtml",Model.NewLaunched)
        </div>
        <div class="clear"></div>
    </div>
</section>

if (Model.HasUpcoming)
{
<section>
    <div class="container section-bottom-margin">
        <h2 class="section-heading">Upcoming bikes in India</h2>
        <div class="grid-12">
            <div class="bg-white content-box-shadow padding-top20">
                @Html.Partial("~/UI/Views/Upcoming/_UpcomingBikes.cshtml", Model.Upcoming)
                <div class="view-all-btn-container padding-top15 padding-bottom20">
                    <a href="/upcoming-bikes/" class="btn view-all-target-btn" title="Upcoming Bikes in India">View all bikes<span class="bwsprite teal-right"></span></a>
                </div>
            </div>
        </div>
        <div class="clear"></div>
    </div>
</section>
}

if (Model.News != null)
{
    <section>
        <div class="container section-bottom-margin">
            <h2 class="section-heading">More about newly launched bikes</h2>
            <div class="grid-12">
                <div class="content-box-shadow content-inner-block-20 card-bottom-margin font14 ">
                    @Html.Partial("~/UI/Views/News/_RecentNews.cshtml", Model.News)
                </div>
            </div>
            <div class="clear"></div>
        </div>
    </section>
    }
}