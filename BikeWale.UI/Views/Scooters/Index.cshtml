                {
                    <div class="bw-tabs-data hide" id="ctrlNewLaunchedBikes">
                        @Html.Partial("~/Views/Shared/_newLaunches.cshtml", NewLaunchesList)
                    </div>
                }
                @if (objUpcoming != null && objUpcoming.Count() > 0)
                {
                    <div class="bw-tabs-data hide" id="ctrlUpcomingBikes">
                        @Html.Partial("~/Views/Shared/_UpcomingBikes.cshtml", objUpcoming)
                    </div>
                }
            </div>
        </div>
        <div class="clear"></div>
    </div>
</section>
@if (TopScootersCompares != null && TopScootersCompares.Count() > 0)
{
    <section>
        @Html.Partial("~/Views/Shared/_popularComparisions.cshtml", TopScootersCompares)
    </section>
}
<script type="text/javascript" src="@(!string.IsNullOrEmpty(ViewBag.staticUrl) ? ("https://st.aeplcdn.com" + @ViewBag.staticUrl) : "")/src/frameworks.js?@ViewBag.staticFileVersion"></script>

