@model Bikewale.Entities.manufacturecampaign.ManufactureCampaignLeadEntity
@using Bikewale.Utility
@if(Model!=null)
{
    <!-- Manufacturer Campaign Ad section starts -->
    try
    {
        @Html.Raw(MvcHelper.Render(String.Format("LeadCampaign_{0}", Model.CampaignId), Model, Model.LeadsHtmlDesktop))
    }
    catch (Exception ex)
    {
        Bikewale.Notifications.ErrorClass.LogError(ex, String.Format("ManufacturerCampaign.Desktop(CampaignId : {0})",Model.CampaignId));
    }
     <!-- Manufacturer Campaign Ad section ends -->
}