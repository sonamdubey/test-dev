@model Bikewale.Entities.manufacturecampaign.ManufactureCampaignLeadEntity
@using Bikewale.Utility
@if(Model!=null)
{
    <!-- Manufacturer Campaign Ad section starts -->
    try
    {
        @Html.Raw(MvcHelper.GetRenderedContent(String.Format("LeadCampaign_{0}", Model.CampaignId), Model.LeadsHtmlDesktop, Model))
    }
    catch (Exception ex)
    {
        Bikewale.Notifications.ErrorClass err = new Bikewale.Notifications.ErrorClass(ex, String.Format("ManufacturerCampaign.Desktop(CampaignId : {0})",Model.CampaignId));
    }
     <!-- Manufacturer Campaign Ad section ends -->
}