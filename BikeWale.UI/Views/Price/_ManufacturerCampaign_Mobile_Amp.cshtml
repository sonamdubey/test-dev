@model Bikewale.Entities.manufacturecampaign.ManufactureCampaignLeadEntity
@using Bikewale.Utility
@if (Model != null)
{
    <!-- Manufacturer Campaign Ad section starts -->
    try
    {
        @Html.Raw(MvcHelper.GetRenderedContent(String.Format("LeadCampaign_Mobile_AMP_{0}", Model.CampaignId), Model.LeadsHtmlMobile, Model).ConvertToAmpContent())
    }
    catch (Exception ex)
    {
        Bikewale.Notifications.ErrorClass.LogError(ex, String.Format("ManufacturerCampaign.Mobile.AMP(CampaignId : {0})", Model.CampaignId));
    }
     <!-- Manufacturer Campaign Ad section ends -->
}