@model Bikewale.Entities.manufacturecampaign.v2.ManufactureCampaignLeadEntity
@using Bikewale.Utility
@if (Model != null)
{
    <!-- Manufacturer Campaign Ad section starts -->
    try
    {
        string hash = Bikewale.PWA.Utils.PwaCmsHelper.GetSha256Hash(Model.LeadsHtmlMobile);
        @Html.Raw(MvcHelper.GetRenderedContent(hash, Model.LeadsHtmlMobile, Model).ConvertToAmpContent())
    }
    catch (Exception ex)
    {
        Bikewale.Notifications.ErrorClass.LogError(ex, String.Format("ManufacturerCampaign.Mobile.AMP(CampaignId : {0})", Model.CampaignId));
    }
     <!-- Manufacturer Campaign Ad section ends -->
}