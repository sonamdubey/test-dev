@model Bikewale.Entities.manufacturecampaign.ManufactureCampaignLeadEntity
@using Bikewale.Utility
@if (Model != null)
{
    <!-- Manufacturer Campaign Ad section starts -->
    try
    {
        @Html.Raw(Format.GetRenderedContent(String.Format("LeadCampaign_Mobile_{0}", Model.CampaignId), Model.LeadsHtmlMobile, Model))
    }
    catch (Exception ex)
    {
        Bikewale.Notifications.ErrorClass err = new Bikewale.Notifications.ErrorClass(ex, String.Format("ManufacturerCampaign.Mobile(CampaignId : {0})", Model.CampaignId));
    }
     <!-- Manufacturer Campaign Ad section ends -->
}