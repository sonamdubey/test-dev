@using Bikewale.Entities.BikeData;
@{
    ViewBag.AdPath = "/1017752/BikeWale_New_";
    ViewBag.AdId = "1395986297721";
    ViewBag.IsHeaderFix = false;
    ViewBag.IsTransparentHeader = false;
    ViewBag.Ad_970x90Bottom = false;
    ViewBag.Ad_300x250BTF = false;
    ViewBag.Ad_300x250 = false;    
    Layout = "~/Views/Shared/_Layout_Desktop.cshtml";
    ViewBag.Page_JS_Path = "/src/new-launch.js";
    ViewBag.Page_ATF_CSS = "<link rel='stylesheet' type='text/css' href='/css/new-launch/new-launch.css' />";
    IEnumerable<UpcomingBikeEntity> objUpcoming = ViewBag.UpcomingBikes;
    ViewBag.staticUrl = Bikewale.Utility.BWConfiguration.Instance.StaticUrl;
    ViewBag.staticFileVersion = Bikewale.Utility.BWConfiguration.Instance.StaticFileVersion;
}
<section>
    <div class="container padding-top10">
        <div class="grid-12">
            <div class="breadcrumb margin-bottom15">
                <ul>
                    <li itemscope="" itemtype="http://data-vocabulary.org/Breadcrumb">
                        <a href="/" itemprop="url" title="Home">
                            <span itemprop="title">Home</span>
                        </a>
                    </li>
                    <li itemscope="" itemtype="http://data-vocabulary.org/Breadcrumb">
                        <span class="bwsprite fa-angle-right margin-right10"></span>
                        <a href="/new-bike-launches/" itemprop="url" title="New bike launches">
                            <span itemprop="title">New bike launches</span>
                        </a>
                    </li>
                    <li itemscope="" itemtype="http://data-vocabulary.org/Breadcrumb">
                        <span class="bwsprite fa-angle-right margin-right10"></span>
                        <span itemprop="title">New @ViewBag.MakeName Bike Launches</span>
                    </li>
                </ul>
                <div class="clear"></div>
            </div>
        </div>
        <div class="clear"></div>
    </div>
</section>
<section>
    <div class="container section-bottom-margin">
        <div class="grid-12">
            <div class="bg-white">
                <h1 class="section-header">New @ViewBag.MakeName Bike Launches</h1>
                <p class="section-inner-padding font14 text-light-grey">@ViewBag.MakeName is a well-known two-wheeler brand in India. The two-wheeler brands in India have to update their portfolio of bikes vis-à-vis changing behavior pattern and usefulness on a regular basis. Every year there are plenty of bike models that hit the market to cater the customer's need. BikeWale brings you an exhaustive list of newly launched @ViewBag.MakeName bikes in India. Explore the list of latest @ViewBag.MakeName bikes.</p>
            </div>
        </div>
        <div class="clear"></div>
    </div>
</section>
<section id="newlaunched-bikes" data-make-filter="@ViewBag.MakeId" data-city="@ViewBag.location.CityId">
    <div class="container section-bottom-margin">
        <div class="grid-12">
            @Html.Partial("~/Views/NewLaunches/_NewLaunchedBikes.cshtml")
        </div>
        <div class="clear"></div>
    </div>
</section>
@if (objUpcoming != null && objUpcoming.Count() > 0)
{
<section>
    <div class="container section-bottom-margin">
        <div class="grid-12">
            <div class="content-box-shadow padding-top20 padding-bottom20">
                <h2 class="font18 padding-bottom20 padding-left20">Upcoming @ViewBag.MakeName bikes in India</h2>
                @Html.Partial("~/Views/Shared/_UpcomingBikes.cshtml", objUpcoming)
            </div>
        </div>
        <div class="clear"></div>
    </div>
</section>
}
<section>
    <div class="container section-bottom-margin">
        <div class="grid-12">
            <div class="content-box-shadow padding-top20 padding-bottom20">
                <h2 class="font18 padding-bottom20 padding-left20">New launches of other brands</h2>               
                @{Html.RenderAction("Makes", "DNewLaunches", new { makeId = @ViewBag.MakeId });}
            </div>
        </div>
        <div class="clear"></div>
    </div>
</section>
<section>
    <div class="container section-bottom-margin">
        <h2 class="section-heading">Explore year-wise bike launches</h2>
        @{Html.RenderAction("Years", "DNewLaunches");}
    </div>
</section> 
<script type="text/javascript" src="@(!String.IsNullOrEmpty(ViewBag.staticUrl) ? ("https://st.aeplcdn.com" + @ViewBag.staticUrl) : "")/src/frameworks.js?@ViewBag.staticFileVersion"></script>