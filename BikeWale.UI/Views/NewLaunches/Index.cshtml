@using Bikewale.Entities.BikeData;
@using Bikewale.Entities.BikeData.NewLaunched;
@{
    ViewBag.Ad_970x90 = false;
    ViewBag.Ad_970x90Bottom = false;
    ViewBag.Ad_300x250BTF = false;
    ViewBag.Ad_Mid_320x50 = false;
    ViewBag.Ad_300x250 = false;
    ViewBag.IsHeaderFix = false;
    ViewBag.IsTransparentHeader = true;
    ViewBag.Page_ATF_CSS = "<link rel='stylesheet' type='text/css' href='/css/new-launch/new-launch.css' />";
    ViewBag.Page_JS_Path = "/src/new-launch.js";
    Layout = "~/Views/Shared/_Layout_Desktop.cshtml";
    IEnumerable<UpcomingBikeEntity> objUpcoming = ViewBag.UpcomingBikes;
    ViewBag.staticUrl = Bikewale.Utility.BWConfiguration.Instance.StaticUrl;
    ViewBag.staticFileVersion = Bikewale.Utility.BWConfiguration.Instance.StaticFileVersion;
}
<header class="new-launch-banner">
    <div id="new-launch-center-box" class="container">
        <div class="welcome-box">
            <h1 class="font30 text-uppercase margin-bottom30">NEW BIKE LAUNCHES - 2017</h1>
            <p class="font20 text-unbold text-white margin-bottom50">Latest bikes in India</p>
        </div>
    </div>
</header>
<section>
    <div class="container">
        <div class="grid-12">
            <div class="content-box-shadow negative-50 padding-15-20 font14 text-light-grey collapsible-content">
                <p class="main-content">India is one of the largest two-wheeler market in the world. The Indian market of two-wheelers has some noteworthy diversity. In order to thrive in this massive competition, manufacturers try to keep up with fast changing trends. Every year there are plenty of bike models that hit the market. BikeWale brings you an exhaustive list of newly launched bikes in India. Explore the list of latest bikes.</p>
            </div>
        </div>
        <div class="clear"></div>
    </div>
</section>
<section>
    <div id="newlaunched-bikes" data-city="@ViewBag.location.CityId" class="container section-bottom-margin">
        <div class="grid-12">
            <h2 class="section-heading">New bike launches in India</h2>
            @Html.Partial("~/Views/NewLaunches/_NewLaunchedBikes.cshtml")
        </div>
        <div class="clear"></div>
    </div>
</section>
<section>
    <div class="container section-bottom-margin">
        <h2 class="section-heading">Explore new launches by brands</h2>
        <div class="grid-12">
            @Html.Partial("~/Views/shared/_newlaunchedbymakewithoutcount.cshtml")
        </div>
        <div class="clear"></div>
    </div>
</section>
@if (objUpcoming != null && objUpcoming.Count() > 0)
{
<section>
    <div class="container section-bottom-margin">
        <h2 class="section-heading">Upcoming bikes in India</h2>
        <div class="grid-12">
            <div class="bg-white content-box-shadow section-bottom-margin padding-top20">
                @Html.Partial("~/Views/Shared/_UpcomingBikes.cshtml", objUpcoming)
                <div class="view-all-btn-container padding-top15 padding-bottom20">
                    <a href="/upcoming-bikes/" class="btn view-all-target-btn" title="Upcoming Bikes in India">View all bikes<span class="bwsprite teal-right"></span></a>
                </div>
            </div>
        </div>
        <div class="clear"></div>
    </div>
</section>
}
<script type="text/javascript" src="@(!string.IsNullOrEmpty(ViewBag.staticUrl) ? ("https://st.aeplcdn.com" + @ViewBag.staticUrl) : "")/src/frameworks.js?@ViewBag.staticFileVersion"></script>