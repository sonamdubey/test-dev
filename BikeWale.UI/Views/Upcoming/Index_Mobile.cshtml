@model Bikewale.Models.UpcomingPageVM
@{
    Layout = "~/Views/Shared/_Layout_Mobile.cshtml";
    Model.Page_ATF_CSS = "<link rel='stylesheet' type='text/css' href='/m/css/upcoming-bikes/upcoming-bikes.css' />";
    Model.Page_JS_Path = "/m/src/upcoming-bikes.js";
    Model.IsTransparentHeader = true;
    Model.IsPageTypeLanding = true;
}

<section>
    <div class="container upcoming-bikes-banner text-center">
        <h1 class="font22 text-uppercase text-white">Upcoming bikes</h1>
        <h2 class="font14 text-unbold text-white">Things to be excited about</h2>
    </div>
</section>

<section>
    <div class="container section-bottom-margin">
        <div class="grid-12">
            <div class="banner-box-shadow padding-right20 padding-left20 padding-bottom20">
                <div class="padding-top15 font14 text-light-grey collapsible-content">
                    <p class="main-content">India is one of the largest two-wheeler markets in the world. Every year the bike enthusiasts around the country eagerly wait for upcoming bikes from different brands. </p>
                    <p class="more-content"> In order to meet the expectation of these young enthusiasts, most of the motorcycle brands announce some of their key expected launches at the beginning of a year. BikeWale is dedicated to serving its users the correct information about the expected launch dates and the price of these upcoming bikes. Explore the list of upcoming
 bikes and get ready to buy your favorite one.</p>
                    <a href="javascript:void(0)" class="read-more-target" rel="nofollow">...Read more</a>
                </div>
            </div>
        </div>
        <div class="clear"></div>
    </div>
</section>

<section>
    <h2 class="section-heading">Upcoming bikes in India</h2>
    <div id="upcoming-bikes" class="container content-box-shadow section-bottom-margin">
        @Html.Partial("~/Views/Upcoming/_UpcomingBikesList_Mobile.cshtml", Model)
    </div>
</section>

<section>
    <h2 class="section-heading">Upcoming bikes by brands</h2>
    <div class="container content-box-shadow section-bottom-margin padding-top25 padding-bottom20 text-center collapsible-brand-content">
        @Html.Partial("~/Views/Make/_Brands_Mobile.cshtml", Model.Brands)
    </div>
</section>

<section>
    <h2 class="section-heading">Newly launched bikes</h2>
    <div class="contaner content-box-shadow section-bottom-margin padding-top15 padding-bottom15">
        @Html.Partial("~/Views/NewLaunches/_NewLaunches_Mobile.cshtml", Model.NewLaunches)
        <div class="view-all-btn-container margin-top10">
            <a href="" title="" class="btn view-all-target-btn">View all launches<span class="bwmsprite teal-right"></span></a>
        </div>
    </div>
</section>