        @functions{
        string GetAdSlot(int slotId,string adPath,string adId){
            switch (slotId)
            {
                case 1:
                    return String.Format("googletag.defineSlot('{0}300x250', [[300, 250]], 'div-gpt-ad-{1}-1').addService(googletag.pubads());",adPath,adId);
                case 2:
                    return String.Format("googletag.defineSlot('{0}300x250_BTF', [[300, 250]], 'div-gpt-ad-{1}-2').addService(googletag.pubads());", adPath, adId);
                case 3:
                    return String.Format("googletag.defineSlot('{0}970x90', [[970, 66], [970, 60], [960, 90], [950, 90], [960, 66], [728, 90], [960, 60], [970, 90]], 'div-gpt-ad-{1}-3').addService(googletag.pubads());", adPath, adId);
                case 4:
                    return String.Format("googletag.defineSlot('{0}970x90_BTF', [[970, 200],[970, 150],[960, 60], [970, 66], [960, 90], [970, 60], [728, 90], [970, 90], [960, 66]], 'div-gpt-ad-{1}-4').addService(googletag.pubads());", adPath, adId);
                case 5:
                    return String.Format("googletag.defineSlot('{0}Bottom_970x90', [[970, 60], [960, 90], [970, 66], [960, 66], [728, 90], [970, 90], [950, 90], [960, 60]], 'div-gpt-ad-{1}-5').addService(googletag.pubads());", adPath, adId);
                case 6:
                    return String.Format("googletag.defineSlot('{0}FirstSlot_976x400',[[976, 150], [976, 100], [976, 250], [976, 300], [976, 350], [976, 400], [970, 90], [976, 200]],'div-gpt-ad-{1}-6').addService(googletag.pubads());", adPath, adId);
                case 7:
                    return string.Format("googletag.defineSlot('{0}SecondSlot_976x400',[[976, 150], [976, 100], [976, 250], [976, 300], [976, 350], [976, 400], [970, 90], [976, 200]],'div-gpt-ad-{1}-7').addService(googletag.pubads());", adPath, adId);
                case 8:
                    return "googletag.defineSlot('/1017752/BikeWale_HomePageNews_FirstSlot_976x204', [[976, 200], [976, 250], [976, 204]], 'div-gpt-ad-1395985604192-8').addService(googletag.pubads());";
                case 9:
                    return string.Format("googletag.defineSlot('{0}_ATF_300x250', [300, 250], 'div-gpt-ad-{1}-9').addService(googletag.pubads());", adPath, adId);
                case 10:
                    return string.Format("googletag.defineSlot('{0}_ATF_970x90', [[970, 90], [728, 90], [950, 90]], 'div-gpt-ad-{1}-10').addService(googletag.pubads());", adPath, adId);
                case 11:
                    return string.Format("googletag.defineSlot('{0}_BTF_300x250', [300, 250], 'div-gpt-ad-{1}-11').addService(googletag.pubads());", adPath, adId);
                case 12:
                    return string.Format("googletag.defineSlot('{0}_BTF_970x90', [[970, 90], [728, 90], [950, 90]], 'div-gpt-ad-{1}-12').addService(googletag.pubads());", adPath, adId);
                case 13:
                    return "googletag.defineSlot('/1017752/Bikewale_Model_Comparo_976x400', [[976, 450], [976, 300], [976, 460], [976, 250], [976, 400]], 'div-gpt-ad-1495092203328-0').addService(googletag.pubads());";
                default:
                    return "";
            }
        }

        string GetGTTargeting(int targetId, string target)
        {
            switch (targetId)
            {
                case 1:
                    return String.Format("googletag.pubads().setTargeting(\"Model\", \"{0}\");", target);
                case 2:
                    return String.Format("googletag.pubads().setTargeting(\"Make\", \"{0}\");", target);
                case 3:
                    return String.Format("googletag.pubads().setTargeting(\"CompareBike-D\", \"{0}\");", target);
                case 4:
                    return String.Format("googletag.pubads().setTargeting(\"City\", \"{0}\");", target);
                default:
                    return "";
            }
        }
        }

        googletag.cmd.push(function () {

            @if(Model.AdTags.Ad_300x250){ @Html.Raw(GetAdSlot(1, Model.AdTags.AdPath, Model.AdTags.AdId))}
            @if(Model.AdTags.Ad_300x250BTF){ @Html.Raw(GetAdSlot(2, Model.AdTags.AdPath, Model.AdTags.AdId))}
            @if(Model.AdTags.Ad_970x90){ @Html.Raw(GetAdSlot(3, Model.AdTags.AdPath, Model.AdTags.AdId))}
            @if(Model.AdTags.Ad_970x90BTF){ @Html.Raw(GetAdSlot(4, Model.AdTags.AdPath, Model.AdTags.AdId))}
            @if(Model.AdTags.Ad_970x90Bottom){ @Html.Raw(GetAdSlot(5, Model.AdTags.AdPath, Model.AdTags.AdId))}
            @if(Model.AdTags.Ad_976x400First){ @Html.Raw(GetAdSlot(6, Model.AdTags.AdPath, Model.AdTags.AdId))}
            @if(Model.AdTags.Ad_976x400Second) { @Html.Raw(GetAdSlot(7, Model.AdTags.AdPath, Model.AdTags.AdId))}
            @if(Model.AdTags.Ad_Model_ATF_300x250){ @Html.Raw(GetAdSlot(9, Model.AdTags.AdPath, Model.AdTags.AdId))}
            @if(Model.AdTags.Ad_Model_ATF_970x90){ @Html.Raw(GetAdSlot(10, Model.AdTags.AdPath, Model.AdTags.AdId))}
            @if(Model.AdTags.Ad_Model_BTF_300x250){ @Html.Raw(GetAdSlot(11, Model.AdTags.AdPath, Model.AdTags.AdId))}
            @if(Model.AdTags.Ad_Model_BTF_970x90){ @Html.Raw(GetAdSlot(12, Model.AdTags.AdPath, Model.AdTags.AdId))}
            @if(Model.AdTags.Ad_976x400_Middle){ @Html.Raw(GetAdSlot(13, Model.AdTags.AdPath, Model.AdTags.AdId))}

        @if(Model.AdTags.Ad_976x204) { @Html.Raw(GetAdSlot(8, Model.AdTags.AdPath, Model.AdTags.AdId))}
            googletag.pubads().enableSingleRequest();
            @if(!String.IsNullOrEmpty(Model.AdTags.TargetedModel)){@Html.Raw(GetGTTargeting(1, Model.AdTags.TargetedModel))}
            @if(!String.IsNullOrEmpty(Model.AdTags.TargetedMakes)){@Html.Raw(GetGTTargeting(2, Model.AdTags.TargetedMakes))}
            @if(!String.IsNullOrEmpty(Model.AdTags.TargetedModels)){@Html.Raw(GetGTTargeting(3, Model.AdTags.TargetedModels))}
            @if(!String.IsNullOrEmpty(Model.AdTags.TargetedCity)){@Html.Raw(GetGTTargeting(4, Model.AdTags.TargetedCity))}

            googletag.pubads().collapseEmptyDivs();
            googletag.pubads().enableSingleRequest();
            googletag.enableServices();
        });
    </script>

