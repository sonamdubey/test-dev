        ViewBag.staticUrl = Bikewale.Utility.BWConfiguration.Instance.StaticUrl;
        ViewBag.staticFileVersion = Bikewale.Utility.BWConfiguration.Instance.StaticFileVersion;
        var Ad_320x50 = Convert.ToBoolean(ViewBag.Ad_320x50!=null? ViewBag.Ad_320x50 : false);
        var Ad_Bot_320x50 = Convert.ToBoolean(ViewBag.Ad_Bot_320x50 !=null ? ViewBag.Ad_Bot_320x50 : false);
        var Ad_300x250 =Convert.ToBoolean(ViewBag.Ad_300x250 !=null ? ViewBag.Ad_300x250 : false);
        var Ad320x150_Top = Convert.ToBoolean(ViewBag.Ad320x150_Top !=null ? ViewBag.Ad320x150_Top : false);
        var Ad320x150_Bottom =Convert.ToBoolean(ViewBag.Ad320x150_Bottom !=null ? ViewBag.Ad320x150_Bottom : false);
        var EnableOG= Convert.ToBoolean(ViewBag.EnableOG !=null ? ViewBag.EnableOG : true);
        var Ad_Mid_320x50 = Convert.ToBoolean(ViewBag.Ad_Mid_320x50 !=null ? ViewBag.Ad_Mid_320x50 : false);
        var IsHomePage= Convert.ToBoolean(ViewBag.IsHomePage !=null ? ViewBag.IsHomePage : false);        
    }
    <meta http-equiv="X-UA-Compatible" content="IE=edge">

    @if (PageMetaTags != null) 
    {
        if (!String.IsNullOrEmpty(PageMetaTags.Title))
        { 
<title>@PageMetaTags.Title</title>
        }
        if (!String.IsNullOrEmpty(PageMetaTags.Description))
        {
<meta name="description" content="@PageMetaTags.Description" />
        }
        if (!String.IsNullOrEmpty(PageMetaTags.Keywords))
        {
<meta name="keywords" content="@PageMetaTags.Keywords" />
        }
        if (EnableOG)
        {
<meta property="og:title" content="@PageMetaTags.Title" />
<meta property="og:type" content="website" />
<meta property="og:description" content="@PageMetaTags.Description" />
<meta property="og:image" content="@(string.IsNullOrEmpty(OGImage) ? Bikewale.Utility.BWConfiguration.Instance.BikeWaleLogo : OGImage)" />
            if (!String.IsNullOrEmpty(PageMetaTags.CanonicalUrl))
            {
<meta property="og:url" content="@PageMetaTags.CanonicalUrl" />
            }
        }
        if (!String.IsNullOrEmpty(PageMetaTags.AlternateUrl))
        {
<meta name="alternate" content="@PageMetaTags.AlternateUrl" />
        }
        if (!String.IsNullOrEmpty(PageMetaTags.CanonicalUrl))
        {
<link rel="canonical" href="@PageMetaTags.CanonicalUrl" />}
        if (!String.IsNullOrEmpty(PageMetaTags.PreviousPageUrl))
        {
<link rel="prev" href="@PageMetaTags.PreviousPageUrl" /> }
        if (!String.IsNullOrEmpty(PageMetaTags.NextPageUrl))
        {
<link rel="next" href="@PageMetaTags.NextPageUrl" />
        }
    }
    else
    {
<title>@Title</title>
<meta name="description" content="@description" />
        if (!String.IsNullOrEmpty(keywords))
        {
<meta name="keywords" content="@keywords" />}
        if (EnableOG)
        {
<meta property="og:title" content="@Title" />
<meta property="og:type" content="website" />
<meta property="og:description" content="@description" />
<meta property="og:image" content="@string.IsNullOrEmpty(OGImage) ? @Bikewale.Utility.BWConfiguration.Instance.BikeWaleLogo : @OGImage" />
            if (!String.IsNullOrEmpty(canonical))
            {
<meta property="og:url" content="@canonical" /> 
            }
        }
        if (!String.IsNullOrEmpty(canonical))
        {
<link rel="canonical" href="@canonical" />}
        if (!String.IsNullOrEmpty(relPrevPageUrl))
        {
<link rel="prev" href="@relPrevPageUrl" />}
        if (!String.IsNullOrEmpty(relNextPageUrl))
        {
<link rel="next" href="@relNextPageUrl" />
        }
    }
                                                  
