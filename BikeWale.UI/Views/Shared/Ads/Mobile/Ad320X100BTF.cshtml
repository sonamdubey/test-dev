
@{
    var AdId = Model.GetType().GetProperty("AdId").GetValue(Model, null);
}

<div id='div-gpt-ad-@string.Format("{0}-7", AdId)' class="margin-bottom15 text-center" style='width:320px; margin:0 auto;'>
    <script>
        googletag.cmd.push(function () { googletag.display('div-gpt-ad-@string.Format("{0}-7", AdId)'); });
    </script>
</div>