@{
    var AdId = Model.GetType().GetProperty("AdId").GetValue(Model, null);
}

<div id='div-gpt-ad-@string.Format("{0}-8", AdId)' style='height:100px; width:300px;'>
    <script type='text/javascript'>
        googletag.cmd.push(function () { googletag.display('div-gpt-ad-@string.Format("{0}-8", AdId)'); });
    </script>
</div>
