@using Bikewale.Entities.BikeData;
@using Bikewale.Entities.BikeData.NewLaunched;
@{
    ViewBag.Page_JS_Path = "/m/src/new-launch.js";

    ViewBag.Page_ATF_CSS = "<link rel='stylesheet' type='text/css' href='/m/css/new-launch/new-launch.css' />";

    Layout = "~/Views/Shared/_Layout_Mobile.cshtml";
    ViewBag.PqSource = (int)Bikewale.Entities.PriceQuote.PQSourceEnum.Mobile_NewLaunchLanding;
    IEnumerable<BikesCountByYearEntityBase> objYears = ViewBag.Years;
    IEnumerable<UpcomingBikeEntity> objUpcoming = ViewBag.UpcomingBikes;
    ViewBag.staticUrl = Bikewale.Utility.BWConfiguration.Instance.StaticUrl;
    ViewBag.staticFileVersion = Bikewale.Utility.BWConfiguration.Instance.StaticFileVersion;
}
<section>
    <div class="container new-launch-banner text-center">
        <h1 class="font22 text-uppercase text-white">@string.Format("NEW BIKE LAUNCHES - {0}", DateTime.Today.Year)</h1>
        <h2 class="font14 text-unbold text-white">Latest bikes in India</h2>
    </div>
</section>
<section>
    <div class="container section-bottom-margin">
        <div class="grid-12">
            <div class="banner-box-shadow padding-right20 padding-left20 padding-bottom20">
                <div class="padding-top15 font14 text-light-grey collapsible-content">
                    <p class="main-content">India is one of the largest two-wheeler market in the world. The Indian market of two-wheelers has some noteworthy diversity. In order to thrive in this massive </p>
                    <p class="more-content">competition, manufacturers try to keep up with fast changing trends. Every year there are plenty of bike models that hit the market. BikeWale brings you an exhaustive list of newly launched bikes in India. Explore the list of latest bikes.</p><a href="javascript:void(0)" class="read-more-target" rel="nofollow">...Read more</a>
                </div>
            </div>
        </div>
        <div class="clear"></div>
    </div>
</section>
@if (ViewBag.Bikes!=null)
{
<section id="newlaunched-bikes" data-city="@ViewBag.location.CityId">
    <h2 class="section-heading">New bike launches in India</h2>
    @Html.Partial("~/Views/m/NewLaunches/_NewLaunchedBikes.cshtml")
</section>
}
<section>
    <h2 class="section-heading">Explore new launches by brands</h2>
    @Html.Partial("~/views/m/shared/_newlaunchedbymakewithoutcount.cshtml")
</section>
@if (objUpcoming != null && objUpcoming.Count() > 0) { 
<section>
    <h2 class="section-heading">Upcoming bikes in India</h2>
    <div class="content-box-shadow padding-top20 padding-bottom20">
        @Html.Partial("~/Views/m/Shared/_UpcomingBikes.cshtml", objUpcoming)
    </div>
</section>
}
<script type="text/javascript" src="@(!string.IsNullOrEmpty(ViewBag.staticUrl) ? ("https://st.aeplcdn.com" + @ViewBag.staticUrl) : "")/m/src/frameworks.js?@ViewBag.staticFileVersion"></script>