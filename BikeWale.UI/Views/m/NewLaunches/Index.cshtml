    ViewBag.Ad_Bot_320x50 = true;
    ViewBag.Page_JS_Path = "/m/src/new-launch.js";

    ViewBag.Page_ATF_CSS = "<link rel='stylesheet' type='text/css' href='/m/css/new-launch/new-launch.css' />";

    Layout = "~/Views/Shared/_Layout_Mobile.cshtml";

    IEnumerable<BikesCountByYearEntityBase> objYears = ViewBag.Years;
    ViewBag.PqSource = (int)Bikewale.Entities.PriceQuote.PQSourceEnum.Mobile_NewLaunchLanding;

