
@{
    ViewBag.Title = "New Bikes Launches";
    ViewBag.AdPath = "/1017752/Bikewale_Mobile_NewBikes";
    ViewBag.AdId = "1398766302464";
    ViewBag.Ad_Bot_320x50 = true;
    ViewBag.staticUrl = Bikewale.Utility.BWConfiguration.Instance.StaticUrl;
    ViewBag.staticFileVersion = Bikewale.Utility.BWConfiguration.Instance.StaticFileVersion;
    Layout = "~/Views/Shared/_Layout_Mobile.cshtml";
    ViewBag.pager = new Bikewale.Entities.Pager.PagerEntity(){
        PageNo = 1,
        PageSize=10,
        PagerSlotSize=5,
        BaseUrl = "/m/new-bikes-launches/",
        PageUrlType = "page/",
        TotalResults = 24
    };
    ViewBag.PqSource = (int)Bikewale.Entities.PriceQuote.PQSourceEnum.Mobile_NewLaunchLanding;
    ViewBag.Page_ATF_CSS = "<link rel='stylesheet' type='text/css' href='/m/css/new-launch/new-launch.css' />";    
}
<section>
    <div class="container new-launch-banner text-center">
        <h1 class="font24 text-uppercase text-white">New bike launches</h1>
        <h2 class="font14 text-unbold text-white">New machines on the block</h2>
    </div>
</section>

<section>
    <div class="container section-bottom-margin">
        <div class="grid-12">
            <div class="banner-box-shadow padding-right20 padding-left20 padding-bottom20">
                <div class="padding-top15 font14 text-light-grey collapsible-content">
                    <p class="main-content">India is one of the largest two-wheeler market in the world. The Indian market of two-wheelers has some noteworthy diversity. In order to thrive in this massive competition, manufacturers try to keep up with fast changing trends.</p>
                    <p class="more-content">Every year there are plenty of bike models that hit the market. BikeWale brings you an exhaustive list of newly launched bikes in India. Explore the list of latest bikes.</p><a href="javascript:void(0)" class="read-more-target" rel="nofollow">...Read more</a>
                </div>
            </div>
        </div>
        <div class="clear"></div>
    </div>
</section>

<section>
    <h2 class="section-heading">New bike launches in India</h2>
    @Html.Partial("~/Views/m/NewLaunches/_NewLaunchedBikes.cshtml", null);
</section>

<section>
    <h2 class="section-heading">Explore new launches by brands</h2>
    @{ Html.RenderAction("Makes", "NewLaunches", new { showCount = false }); }     
</section>

<section>
    <h2 class="section-heading">Explore year-wise bike launches</h2>
    @{ Html.RenderAction("Years", "NewLaunches"); }
    
</section>

