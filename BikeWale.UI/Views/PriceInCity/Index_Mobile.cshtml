                        <tr>
                            <td align="left" valign="top" width="70%" class="padding-bottom15 text-slate-grey">Ex-showroom</td>
                            <td align="right" valign="top" width="30%" class="padding-bottom15 text-bold"><span class="bwmsprite inr-dark-grey-xsm-icon"></span><span data-bind="text: exshowroomPrice()"></span></td>
                        </tr>
                        <!-- /ko -->
                        <!--  ko if: rtoPrice().length > 0 -->
                        <tr>
                            <td align="left" valign="top" class="padding-bottom15 text-slate-grey">RTO</td>
                            <td align="right" valign="top" class="padding-bottom15 text-bold"><span class="bwmsprite inr-dark-grey-xsm-icon"></span><span data-bind="text: rtoPrice()"></span></td>
                        </tr>
                        <!-- /ko -->
                        <!--  ko if: insurancePrice().length > 0 -->
