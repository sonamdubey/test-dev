    Model.AdTags.Ad_Bot_320x50 = true;
    Model.AdTags.AdPath = "/1017752/Bikewale_Mobile_Model";
    Model.AdTags.AdId = "1444028976556";
    Model.PageMetaTags.AlternateUrl = "";
    Model.Page_ATF_CSS = "<link rel='stylesheet' type='text/css' href='/m/css/new/bwm-modelprice-in-city.css' />";
    Model.Page_BTF_CSS_Path = "/m/css/bwm-common-btf.css";
    Model.Page_JS_Path = "/m/src/priceincity.js";
    Layout = "~/Views/Shared/_Layout_Mobile.cshtml";
}


<section>
    <div class="container">
        <div class="padding-top10 padding-left20 padding-right20 bg-white">
            <h1 class="font20">@Model.Page_H1</h1>
        </div>
        <div id="modelSpecsTabsContentWrapper">
            <div id="modelOverallSpecsTopContent" class="bg-white position-rel">
                <div id="overallSpecsTab" class="overall-specs-tabs-container">
                    <ul class="overall-specs-tabs-wrapper">
                        <li data-tabs="#orpContent">On-road price</li>
                        <li data-tabs="#aboutContent">About</li>
                        <li data-tabs="#dealerContent">Dealers</li>
                        <li data-tabs="#serviceCenterContent">Service centers</li>
                        <li data-tabs="#emiContent">EMI calculator</li>
                        <li data-tabs="#versionContent">Versions</li>
                        <li data-tabs="#nearbyCityContent">Nearby cities</li>
                        <li data-tabs="#similarContent">Similar bikes</li>
                        <li data-tabs="#otherCityContent">Other cities</li>
                    </ul>
                </div>
            </div>

            <!-- model orp start -->
            <div id="orpContent" class="bw-model-tabs-data content-box-shadow card-bottom-margin content-inner-block-20">
                <div class="margin-bottom15">
                    <div class="model-image inline-block">
                        <img src="@Model.ModelImage" title="@Model.BikeName" alt="@Model.BikeName" />
                    </div>
                    <div class="model-version-block inline-block padding-left15">
                        <p class="font12 text-light-grey">Version:</p>
                        <div class="dropdown-select-wrapper">
                            <select class="dropdown-select" data-title="Version">
                                <option value="4514">2017</option>
                                <option value="111">Kick/Drum/Spokes</option>
                                <option value="112">Electric Start/Drum/Alloy</option>
                            </select>
                        </div>
                    </div>
                </div>

