@model Bikewale.Models.PriceInCityPageVM
@using Bikewale.Utility;
@using Bikewale.Common;
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    Model.Page_ATF_CSS = "<link rel='stylesheet' type='text/css' href='/css/new/modelprice-in-city.css' />";
    Model.Page_JS_Path = "/src/priceincity.js";
    Model.AdTags.Ad_970x90 = true;
    Model.AdTags.Ad_970x90Bottom = true;
    Model.AdTags.AdId = "1442913773076";
    Model.AdTags.AdPath = "/1017752/Bikewale_NewBike_";
    Model.IsPageTypeLanding = false;
    Model.IsHeaderFix = false;
}
@if (Model != null && Model.BikeModel != null && Model.CityEntity != null)
{
    <section class="bg-light-grey padding-top10">
        <div class="container">
            <div class="grid-12">
                <div class="breadcrumb margin-bottom15">
                    <!-- breadcrumb code starts here -->
                    <ul>
                        <li itemscope="" itemtype="http://data-vocabulary.org/Breadcrumb">
                            <a href="/" itemprop="url">
                                <span itemprop="title">Home</span>
                            </a>
                        </li>
                        <li itemscope="" itemtype="http://data-vocabulary.org/Breadcrumb">
                            <span class="bwsprite fa-angle-right margin-right10"></span>
                            <a href="/@Model.Make.MaskingName-bikes/" itemprop="url">
                                <span itemprop="title">@Model.Make.MakeName Bikes</span>
                            </a>
                        </li>
                        <li>
                            <span class="bwsprite fa-angle-right margin-right10"></span>
                            <a href="@UrlFormatter.BikePageUrl(Model.Make.MaskingName,Model.BikeModel.MaskingName)" itemprop="url">
                                <span>@Model.BikeName</span>
                            </a>
                        </li>
                        <li>
                            <span class="bwsprite fa-angle-right margin-right10"></span>
                            <span>@String.Format("Price in {0}", Model.CityEntity.CityName)</span>
                        </li>
                    </ul>
                    <div class="clear"></div>
                </div>
            </div>
            <div class="clear"></div>
        </div>
    </section>

    <section>
        <div class="container">
            <div class="grid-12">
                <div class="padding-top15 padding-left20 content-box-shadow">
                    <h1>@Model.Page_H1</h1>
                </div>
                <div id="modelSpecsTabsContentWrapper">
                    <div id="modelOverallSpecsTopContent" class="bg-white position-rel">
                        <div id="overallSpecsTab" class="overall-specs-tabs-container">
                            <div class="overall-specs-tabs-wrapper">
                              @Html.Partial("~/views/PriceInCity/_Index_Tabs.cshtml")
                            </div>
                        </div>
                    </div>

                    <!-- model on-road price start -->
                    <div id="orpContent" class="bw-model-tabs-data">
                        <div class="content-box-shadow content-inner-block-20 section-bottom-margin">
                            <div class="grid-8 alpha omega orp-grid-8">
                                <div class="orp-left-grid leftfloat margin-top5 padding-right10">
                                    <div class="model-image position-rel margin-bottom10">
                                        @if (Model.IsDiscontinued)
                                        {
                                            <p class="model-ribbon  discontinued-ribbon">Discontinued</p>
                                        }
                                        <img src="@Model.ModelImage" title="@Model.BikeName" alt="@Model.BikeName" />
                                    </div>
                                    <ul id="version-specs-list">
                                        <li>Alloy Wheels</li>
                                        <li>Electric Start</li>
                                        <li>Disc Brake</li>
                                    </ul>
                                </div>
                                <div class="orp-right-grid leftfloat">
                                    <div id="model-version-dropdown" class="margin-right15 padding-top20 vertical-top">
                                        <div class="select-box select-box-no-input done size-small">
                                            <p class="select-label">Version</p>
                                     <select id="version-dropdown" @*class="chosen-select"*@ data-title="Version" data-bind="options: bikeVersions, value: selectedVersion, optionsText: 'VersionName', optionsValue: 'VersionId'">
                                     </select>
                                        </div>
                                    </div>
                                    <div class="location-selection vertical-top cur-pointer">
                                        <p class="font12 text-light-grey">City</p>
                                        <p class="text-bold selected-city text-truncate position-rel">
                                            <span>@Model.CityEntity.CityName</span>
                                            <a href="javascript:void(0)" data-pqsourceid="@((int) Bikewale.Entities.PriceQuote.PQSourceEnum.Desktop_PriceInCity_SelectAreas)" f="getBikeVersionName" c="Price_in_City_Page" a="Select_Area_Clicked" data-preselcity="@Model.CityEntity.CityId" data-persistent="true" data-modelid="@Model.BikeModel.ModelId" class="getquotation changeCity bw-ga" rel="nofollow">
                                            <span class="bwsprite loc-change-blue-icon"></span></a>
                                        </p>
                                    </div>

                                    <table width="100%" cellspacing="0" cellpadding="0" class="margin-top15 font14">
                                        <thead>
                                            <tr>
                                                <th colspan="2" align="left" class="padding-bottom10 text-default text-bold">@Model.BikeName price in @Model.CityEntity.CityName</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <!-- ko if: exshowroomPrice().length > 0 -->
                                            <tr>
                                                <td align="left" valign="top" width="70%" class="padding-bottom10 text-slate-grey">Ex-showroom</td>
                                                <td align="right" valign="top" width="30%" class="padding-bottom10 text-bold"><span class="bwsprite inr-sm"></span><span data-bind="text: exshowroomPrice()"></span></td>
                                            </tr>
                                            <!-- /ko -->
                                            <!-- ko if: rtoPrice().length > 0 -->
                                            <tr>
                                                <td align="left" valign="top" width="70%" class="padding-bottom10 text-slate-grey">RTO</td>
                                                <td align="right" valign="top" width="30%" class="padding-bottom10 text-bold"><span class="bwsprite inr-sm"></span><span data-bind="text: rtoPrice()"></span></td>
                                            </tr>
                                            <!-- /ko -->
                                            <!-- ko if: insurancePrice().length > 0 -->
                                            <tr>
                                                <td align="left" valign="top" width="70%" class="padding-bottom15 text-slate-grey">Insurance (Comprehensive)</td>
                                                <td align="right" valign="top" width="30%" class="padding-bottom15 text-bold"><span class="bwsprite inr-sm"></span><span data-bind="text: insurancePrice()"></span></td>
                                            </tr>
                                            <!-- /ko -->
                                            <tr>
                                                <td align="left" valign="top" width="70%" class="padding-top15 border-solid-top">@Model.BikeName price in @Model.CityEntity.CityName</td>
                                                <td align="right" valign="top" width="30%" class="padding-top15 border-solid-top font16 text-bold"><span class="bwsprite inr-md"></span><span data-bind="text: onRoadPrice()"></span></td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                                <div class="clear"></div>
                            </div>
                            <div class="grid-4 padding-left5 omega padding-top30">
                                <div class="padding-left10">                                   
                               @if (Model.IsAreaSelected && Model.IsNew){
                                    <div class="margin-bottom15">
                                      <span class="dealer-icon"></span>
                                        <div class="inline-block dealer-icon-label">
                                            <h3 class="font-bold">Executive Bajaj, Ghatkopar (W)</h3>
                                            <p class="font12 text-xx-light">Ghatkopar (W)</p>
                                        </div>
                                    </div>
                                    <div class="text-slate-grey font14">
                                        <p class="margin-bottom10">Get in touch with Executive Bajaj for:</p>
                                        <ul class="list-type-disc margin-bottom15">
                                            <li>Best offers</li>
                                            <li>EMI options</li>
                                            <li>Complete buying assistance</li>
                                        </ul>
                                        <a href="" class="btn btn-orange btn-180-32">View dealer details</a>
                                    </div>
                               }
                               else if (Model.IsAreaAvailable && Model.IsNew)
                               {
                                <p class="text-black">Please select your area to get:</p>
                                <ul class="selectAreaToGetList margin-bottom20">
                                    <li class="bullet-point">
                                        <p>Nearest dealership details</p>
                                    </li>
                                    <li class="bullet-point">
                                        <p>Exclusive offers</p>
                                    </li>
                                    <li class="bullet-point">
                                        <p>Complete buying assistance</p>
                                    </li>
                                </ul>
                                <a href="javascript:void(0)" data-pqsourceid="@((int) Bikewale.Entities.PriceQuote.PQSourceEnum.Desktop_PriceInCity_SelectAreas)" f="getBikeVersionName" c="Price_in_City_Page" a="Select_Area_Clicked" data-preselcity="@Model.CityEntity.CityId" data-persistent="true" data-modelid="@Model.BikeModel.ModelId" class="btn btn-orange btn-xxlg font14 getquotation changeCity bw-ga" rel="nofollow">Select your area</a>
                               }
                               else
                               {
                                <script type='text/javascript' src='https://www.googletagservices.com/tag/js/gpt.js'>
                                    googletag.pubads().definePassback('/1017752/Bikewale_PQ_300x250', [300, 250]).display();
                                </script>
                               }
                                </div>
                            </div>

                            @*<div class="grid-4 padding-top15 padding-left15">
                                @if (Model.IsAreaSelected && Model.IsNew)
                                {
                                    <p class="text-black">Get in touch with dealer for:</p>
                                    <ul class="selectAreaToGetList margin-bottom20">
                                        <li class="bullet-point">
                                            <p>Best offers</p>
                                        </li>
                                        <li class="bullet-point">
                                            <p>Test rides</p>
                                        </li>
                                        <li class="bullet-point">
                                            <p>Complete buying assistance</p>
                                        </li>
                                    </ul>
                                    <a id="dealerDetails" href="javascript:void(0)" data-pqsourceid="@((int) Bikewale.Entities.PriceQuote.PQSourceEnum.Desktop_PriceInCity_Dealer_Detail_Click)" f="getBikeVersionName" c="Price_in_City_Page" a="View_dealer_details_Clicked" data-preselcity="@Model.CityEntity.CityId" data-modelid="@Model.BikeModel.ModelId" class="btn btn-orange btn-xxlg font14 bw-ga" rel="nofollow">View dealer details</a>
                                }
                                else if (Model.IsAreaAvailable && Model.IsNew)
                                {
