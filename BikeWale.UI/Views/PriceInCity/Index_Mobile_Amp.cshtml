@model Bikewale.Models.PriceInCityPageVM
@using Newtonsoft.Json
@{
    Layout = "~/Views/Shared/_Layout_Mobile_AMP.cshtml";
    Model.Amp_Page_CSS = "<link rel='stylesheet' type='text/css' href='/m/css/amp/bwm-modelprice-in-city-amp.css' />";
}
<amp-state id="currentVersion">
    <script type="application/json">
        {
        "versionId" : @JsonConvert.SerializeObject(Model.FirstVersion.VersionId)
        }
    </script>
</amp-state>
<amp-state id="versions">
    <script type="application/json">
        @{
            Dictionary<string, Bikewale.Entities.PriceQuote.BikeQuotationEntity> values = new Dictionary<string, Bikewale.Entities.PriceQuote.BikeQuotationEntity>();
            //values.Add("currentVersion", Model.FirstVersion);
            foreach (var item in Model.BikeVersionPrices)
            {
                values.Add(Convert.ToString(item.VersionId), item);
            }
        }
        @Html.Raw(JsonConvert.SerializeObject(values))
        @*{
            "currentVersion": "1",
            "1": {
            "name": "Standard",
            "exshowroom": "1,33,932",
            "RTO": "27,347",
            "insurance": "3,784",
            "onRoadPrice": "1,65,063"

            },
            "2": {
            "name": "Redditch Edition",
            "exshowroom": "43,35,559",
            "RTO": "57,681",
            "onRoadPrice": "3,65,063"
            }
            }*@
    </script>
</amp-state>
<amp-state id="Popup">
    <script type="application/json">
        {
        "totalAmount":"201169",
        "formatedTotalAmount":"0",
        "downPayment": "50122" ,
        "formatedDownPayment":"0",
        "loanAmount" : "116952",
        "formatedLoanAmount" : "0",
        "tenure" :30,
        "interest" :12.5,
        "fees":0,
        "bikePrice" : 167074,
        "emi": "0" ,
        "formatedEMI":"0",
        "city" : "Select City"
        }
    </script>
</amp-state>
@if (Model != null && Model.BikeModel != null && Model.CityEntity != null)
{

}
<section id="priceincity">
    <div class="container">
        <h1 class="price-in-city__head">@Model.Page_H1</h1>
        <div class="modelSpecsTabsContentWrapper">
            <div id="modelOverallSpecsTopContent" class="position-rel">
                <div id="overallSpecsTab">
                    @Html.Partial("~/views/priceincity/_index_Tabs_Mobile_Amp.cshtml", Model)
                </div>
            </div>
        </div>
        <!-- model orp start -->
        <div id="orpContent" class="bw-model-tabs-data active">
            <div class="orpContent-block">
                <div class="margin-bottom15">
                    <div class="model-image">
                        @if (Model.IsDiscontinued)
                        {
                            <p class="model-ribbon-tag discontinued-ribbon">Discontinued</p>
                        }
                        @{string returnUrl = string.Format("/m/{0}-bikes/{1}/price-in-{2}/", Model.Make.MaskingName, Model.BikeModel.MaskingName, Model.CityEntity.CityMaskingName); }
                        <a href="@String.Format(@"/m{0}?q={1}", Bikewale.Utility.UrlFormatter.FormatPhotoPageUrl(Model.Make.MaskingName, Model.BikeModel.MaskingName), Bikewale.Utility.EncodingDecodingHelper.EncodeTo64(string.Format("retUrl={0}", returnUrl)))">
                            <amp-img src="@Model.ModelImage" layout="responsive" width="86" height="48" alt="@Model.BikeName"></amp-img>
                        </a>
                    </div>
                    <div class="model-version-block">
                        <p class="version-block__title">Version:</p>
                        @if (Model.BikeVersionPrices.Count() > 1)
                        {
                            <div class="dropdown-select-wrapper">
                                <p id="select-box" role="option" tabindex="1" [text]="versions[currentVersion.versionId].VersionName" class="select-box" on="tap:select-option.show">@Model.FirstVersion.VersionName</p>
                                <amp-selector class="select-wrapper__list" layout="container">
                                    <div id="select-option" hidden>
                                        <div role="option" class="text-bold" [class]="versions[currentVersion.versionId].VersionName == '@Model.FirstVersion.VersionName' ? 'text-bold':'show'" tabindex="0" option="@Model.FirstVersion.VersionId" on="tap:select-option.hide,AMP.setState({currentVersion: {versionId : '@Model.FirstVersion.VersionId'}})">@Model.FirstVersion.VersionName</div>
                                        @foreach (var item in Model.BikeVersionPrices)
                                        {
                                            if (Model.FirstVersion.VersionId != item.VersionId)
                                            {
                                                <div role="option" class="show" [class]="versions[currentVersion.versionId].VersionName == '@item.VersionName' ? 'text-bold':'show'" tabindex="0" option="@item.VersionId" on="tap:select-option.hide,AMP.setState({currentVersion: {versionId : '@item.VersionId'}})">@item.VersionName</div>
                                            }
                                        }
                                        @*<div role="option" class="text-bold" [class]="versions[versions.currentVersion].name == 'Standard' ? 'text-bold':'show'" tabindex="0" option="1" on="tap:select-option.hide,AMP.setState({versions:{ currentVersion:'1'}})">Standard</div>
                                            <div role="option" class="show" [class]="versions[versions.currentVersion].name == 'Redditch Edition' ? 'text-bold':'show'" tabindex="0" option="2" on="tap:select-option.hide,AMP.setState({versions:{ currentVersion:'2'}})">Redditch Edition</div>*@
                                    </div>
                                </amp-selector>
                            </div>
                        }
                        else
                        {
                            <div class="single-version-content font14">
                                <p class="dropdown-effect-light">@Model.FirstVersion.VersionName</p>
                            </div>
                        }
                    </div>
                </div>
                @if (Model.IsNew)
                {
                    <table class="model-version_price-table">
                        <thead>
                            <tr>
                                <th colspan="2" class="price-table__heading">@string.Format("{0} price in {1}", Model.BikeName, Model.CityEntity.CityName)</th>
                            </tr>
                        </thead>
                        <tbody class="price-table__body">
                            <tr class="show" [class]="versions[currentVersion.versionId].ExShowroomPrice ? 'show': 'hide'">
                                <td align="left" valign="top" width="70%">Ex-showroom</td>
                                <td align="right" valign="top" width="30%"><span>&#x20b9;</span>&nbsp;<p class="text-bold inline-block" [text]="versions[currentVersion.versionId].ExShowroomPrice">@Model.FirstVersion.ExShowroomPrice</p></td>
                            </tr>
                            <tr class="show" [class]="versions[currentVersion.versionId].RTO ? 'show': 'hide'">
                                <td align="left" valign="top">RTO</td>
                                <td align="right" valign="top"><span>&#x20b9;</span>&nbsp;<p class="text-bold inline-block" [text]="versions[currentVersion.versionId].RTO">@Model.FirstVersion.RTO</p></td>
                            </tr>
                            <tr class="show" [class]="versions[currentVersion.versionId].Insurance ? 'show': 'hide'">
                                <td align="left" valign="top">
                                    Insurance (Comprehensive)<br><p class="position-rel">
                                    </p>
                                </td>
                                <td align="right" valign="top"><span>&#x20b9;</span>&nbsp;<p class="text-bold inline-block" [text]="versions[currentVersion.versionId].Insurance">@Model.FirstVersion.Insurance</p></td>
                            </tr>
                            <tr class="show" [class]="versions[currentVersion.versionId].OnRoadPrice ? 'show': 'hide'">
                                <td align="left" valign="top">@string.Format("{0} on-road price in {1}", Model.BikeModel.ModelName, Model.CityEntity.CityName)</td>
                                <td align="right" valign="top" class="font16"><span>&#x20b9;</span>&nbsp;<p class="text-bold inline-block" [text]="versions[currentVersion.versionId].OnRoadPrice">@Model.FirstVersion.OnRoadPrice</p></td>
                            </tr>
                        </tbody>
                    </table>
                    <div class="border-solid-bottom padding-bottom15">
                        <span class="bwmsprite info-grey-14"></span>
                        <span class="text-light-grey font12">@string.Format("{0}", Model.FirstVersion.IsGstPrice ? "Price in accordance with GST rates" : "Estimated GST price as dealer price is unavailable.")</span>
                    </div>

                    <div class="margin-top15 font14">
                        <p class="font-bold margin-bottom10">@string.Format("Quick facts about {0}", Model.BikeModel.ModelName)</p>
                        <p class="text-slate-grey line-height16">@Html.Raw(Model.PageDescription)</p>
                    </div>
                }
                @if (Model.IsDiscontinued)
                {
                    <div class="font14 text-default text-bold">
                        <span>Last known Ex-showroom price</span><span class="rightfloat">@Html.Raw("<span class='font16 text-bold'> &#x20B9;</span>")<span class="text-bold font16" data-bind="text: exshowroomPrice()">@Model.FirstVersion.ExShowroomPrice</span></span>
                    </div>
                    <div class="margin-top15 font14">
                        <p class="text-slate-grey line-height16">@String.Format("{0} has discontinued the {1} and the bike is out of production.", Model.Make.MakeName, Model.BikeModel.ModelName)</p>
                    </div>
                }
            </div>
            <!-- model about start -->
            <div class="content-box-shadow card-bottom-margin">
                @Html.Partial("~/views/BikeModels/_BikeInfoCard_Mobile_AMP.cshtml", Model.BikeInfo)
            </div>
            <!-- model about end -->
        </div>
        <!-- model orp end -->
        <!-- model dealers start -->
        @if (Model.HasDealers || Model.HasServiceCenters || Model.HasCampaignDealer)
        {
            <div id="dealerContent" class="bw-model-tabs-data card-bottom-margin">

                <div class="content-box-shadow padding-top20 padding-bottom5 card-bottom-margin">
                    <div class="carousel-heading-content">
                        <div class="swiper-heading-left-grid inline-block">
                            <h2>Royal Enfield Showrooms in Bangalore</h2>
                        </div>
                        <div class="swiper-heading-right-grid inline-block text-right">
                            <a href="/m/royalenfield-dealer-showrooms-in-bangalore/" title="Royal Enfield Showrooms in Bangalore" class="btn view-all-target-btn">View all</a>
                        </div>
                    </div>
                    <amp-carousel class="dealer-horizontal-swiper" height="180" layout="fixed-height" type="carousel">
                        <div class="swiper-card">
                            <a href="/m/royalenfield-dealer-showrooms-in-bangalore/14953-acclaim-motors-re-yelahanka/" title="Acclaim Motors RE - Yelahanka" class="block">
                                <div class="dealer-name">Acclaim Motors RE - Yelahanka</div>
                                <p class="margin-bottom10 text-light-grey ">
                                    <span class="bwmsprite dealership-loc-icon vertical-top"></span>
                                    <span class="dealership-address">G-1, Pyramid North Square, No.<br>520 / 432, Bellary Road, <br> Nehru Nagar</span>
                                </p>
                            </a>
                            <a href="tel:9420232788" class="dealer-contact"><span class="bwmsprite tel-sm-grey-icon"></span>9420232788</a>
                        </div>
                        <div class="swiper-card">
                            <a href="/m/royalenfield-dealer-showrooms-in-bangalore/14953-acclaim-motors-re-yelahanka/" title="Acclaim Motors RE - Yelahanka" class="block">
                                <div class="dealer-name">Acclaim Motors RE - Yelahanka</div>
                                <p class="margin-bottom10 text-light-grey ">
                                    <span class="bwmsprite dealership-loc-icon vertical-top"></span>
                                    <span class="dealership-address">
                                        G-1, Pyramid North Square, No.<br>520 / 432, Bellary Road, <br> Nehru Nagar
                                    </span>
                                </p>
                            </a>
                            <a href="tel:9420232788" class="dealer-contact"><span class="bwmsprite tel-sm-grey-icon"></span>9420232788</a>
                        </div>
                    </amp-carousel>
                </div>
                <!-- model service centers start -->
                @if (Model.HasServiceCenters)
                {
                    <div class="content-box-shadow card-bottom-margin content-inner-block-20">
                        <div class="margin-bottom10">
                            <span class="bwmsprite service-center-grey"></span>
                            <div class="grid-11-inline padding-left10">
                                <h2 class="card-h2-heading">@String.Format("{0} {1}", Model.ServiceCentersCount, Model.ServiceCenterWidget_H2)</h2>
                            </div>
                        </div>
                        <p class="font14 text-slate-grey margin-bottom15">Get in touch with your nearest @Model.Make.MakeName service center in @Model.CityEntity.CityName for service enquiries, service and repair costs and more!</p>
                        <a href="@String.Format("/m{0}", Model.ServiceCenterWidget_ViewAll_Href)" title="@Model.ServiceCenterWidget_ViewAll_Title" class="btn view-all-target-btn">View complete list<span class="bwmsprite teal-right"></span></a>
                    </div>
                }
                <!-- model service centers end -->
            </div>
        }

        <!-- model emi calculator start -->
        @if (Model.IsNew)
        {
            <div id="emiContent" class="bw-model-tabs-data content-box-shadow card-bottom-margin content-inner-block-20">
                <h2 class="card-h2-heading margin-bottom10">@Model.BikeModel.ModelName EMI Calculator</h2>
                <div class="finance-emi-container font14">
                    <div class="emi-slider-box">
                        <div class="emi-slider-box-left-section">
                            <p class="grid-8 alpha text-slate-grey font14">Down payment</p>
                            <div class="emi-slider-box-right-section grid-4 omega">
                                <span class="bwmsprite inr-xxsm-icon margin-right5"></span><span id="downPayment" class="font14 text-bold">50,122</span>
                                <span class="hide" [class]="((Popup.formatedDownPayment.lastIndexOf('.'))>5) ? 'text-bold': 'hide'" [text]="Popup.formatedDownPayment.lastIndexOf('.')-5 <=1? Popup.formatedDownPayment.substr(Popup.formatedDownPayment.lastIndexOf('.')-6, 1)+',' : Popup.formatedDownPayment.substr(Popup.formatedDownPayment.lastIndexOf('.')-7, 2)+','">0,</span>
                                <span class="hide" [class]="((Popup.formatedDownPayment.lastIndexOf('.'))>3) ? 'text-bold': 'hide'" [text]="Popup.formatedDownPayment.lastIndexOf('.')-3 <=1? Popup.formatedDownPayment.substr(Popup.formatedDownPayment.lastIndexOf('.')-4, 1) : Popup.formatedDownPayment.substr(Popup.formatedDownPayment.lastIndexOf('.')-5, 2)">0</span>
                                <span class="hide" [class]="((Popup.formatedDownPayment.lastIndexOf('.')) > 3) ? 'text-bold': 'hide'" [text]="Popup.formatedDownPayment.lastIndexOf('.')>3? ',' + Popup.formatedDownPayment.substr(Popup.formatedDownPayment.lastIndexOf('.')-3,3) : Popup.formatedDownPayment.substr(Popup.formatedDownPayment.lastIndexOf('.')-3,3)">0</span>
                            </div>
                            <div class="clear"></div>
                            <input id="downPaymentSlider" class="margin-bottom20" type="range" min="16707" max="66829" step="1" value="50122" [value]="Popup.downPayment" on="change:downPayment.hide, AMP.setState({Popup:{downPayment:(event.value),formatedDownPayment:event.value+'.', totalAmount:(Popup.bikePrice-event.value)+Popup.fees+((Popup.bikePrice-event.value) * Popup.tenure * Popup.interest / 1200)}}); input-debounced:AMP.setState({Popup:{emi:round(Popup.totalAmount/Popup.tenure)+ '.', loanAmount:Popup.bikePrice-event.value, formatedTotalAmount:round(Popup.totalAmount) + '.', formatedLoanAmount:Popup.bikePrice-event.value+'.'} }), totalemi.hide, totalAmount.hide, loanAmount.hide" />
                        </div>
                        <div class="clear"></div>
                    </div>
                    <div class="emi-slider-box font14">
                        <div class="emi-slider-box-left-section">
                            <p class="grid-8 alpha text-slate-grey font14">Loan amount</p>
                            <div class="emi-slider-box-right-section grid-4 omega">
                                <span class="bwmsprite inr-xxsm-icon margin-right5"></span><span id="loanAmount" class="text-bold font14">1,16,952</span>
                                <span class="hide" [class]="((Popup.formatedLoanAmount.lastIndexOf('.'))>5) ? 'text-bold': 'hide'" [text]="Popup.formatedLoanAmount.lastIndexOf('.')-5 <=1? Popup.formatedLoanAmount.substr(Popup.formatedLoanAmount.lastIndexOf('.')-6, 1)+',' : Popup.formatedLoanAmount.substr(Popup.formatedLoanAmount.lastIndexOf('.')-7, 2)+','">0,</span>
                                <span class="hide" [class]="((Popup.formatedLoanAmount.lastIndexOf('.'))>3) ? 'text-bold': 'hide'" [text]="Popup.formatedLoanAmount.lastIndexOf('.')-3 <=1? Popup.formatedLoanAmount.substr(Popup.formatedLoanAmount.lastIndexOf('.')-4, 1) : Popup.formatedLoanAmount.substr(Popup.formatedLoanAmount.lastIndexOf('.')-5, 2)">0</span>
                                <span class="hide" [class]="((Popup.formatedLoanAmount.lastIndexOf('.')) > 3) ? 'text-bold': 'hide'" [text]="Popup.formatedLoanAmount.lastIndexOf('.')>3? ',' + Popup.formatedLoanAmount.substr(Popup.formatedLoanAmount.lastIndexOf('.')-3,3) : Popup.formatedLoanAmount.substr(Popup.formatedLoanAmount.lastIndexOf('.')-3,3)">0</span>
                            </div>
                            <div class="clear"></div>
                            <input id="loanAmountSlider" class="margin-bottom20" type="range" min="100693" max="150366" step="1" value="116952" [value]="Popup.loanAmount" on="change:AMP.setState({Popup:{loanAmount:event.value, downPayment:Popup.bikePrice-event.value, totalAmount:event.value+Popup.fees+(event.value * Popup.tenure * Popup.interest / 1200)}});input-debounced:AMP.setState({Popup:{ emi:round(Popup.totalAmount/Popup.tenure)+ '.' , formatedTotalAmount:round(Popup.totalAmount) + '.', formatedDownPayment:Popup.downPayment+'.', formatedLoanAmount:event.value+'.'}}),totalAmount.hide, totalemi.hide, downPayment.hide, loanAmount.hide" />
                        </div>
                        <div class="clear"></div>
                    </div>
                    <div class="emi-slider-box">
                        <div class="emi-slider-box-left-section">
                            <p class="grid-8 alpha text-slate-grey font14">Tenure</p>
                            <div class="emi-slider-box-right-section grid-4 omega text-bold">
                                <span id="tenurePeriod" [text]="Popup.tenure">30</span>&nbsp;Months
                            </div>
                            <div class="clear"></div>
                            <input class="margin-bottom20" id="tenureSlider" type="range" min="12" max="48" step="1" value="30" on="change:AMP.setState({Popup: {tenure:event.value, totalAmount:(+Popup.loanAmount)+Popup.fees+(event.value * Popup.loanAmount * Popup.interest / 1200)}}); input-debounced:AMP.setState({Popup:{  emi:round(Popup.totalAmount/Popup.tenure)+ '.' , formatedTotalAmount:round(Popup.totalAmount) + '.'} }),totalAmount.hide, totalemi.hide" />
                        </div>
                        <div class="clear"></div>
                    </div>
                    <div class="emi-slider-box">
                        <div class="emi-slider-box-left-section">
                            <p class="grid-8 alpha text-slate-grey font14">Interest</p>
                            <div class="emi-slider-box-right-section grid-4 omega text-bold ">
                                <span id="rateOfInterestPercentage" [text]="Popup.interest">12.5</span>&nbsp;%
                            </div>
                            <div class="clear"></div>
                            <input class="margin-bottom20" id="rateOfInterestSlider" type="range" min="10" max="15" step="0.25" value="12.5" on="change:AMP.setState({Popup: {interest:event.value, ta:(+Popup.loanAmount)+Popup.fees+(Popup.tenure * Popup.loanAmount * event.value / 1200)}}); input-debounced:AMP.setState({Popup:{  emi:round(Popup.ta/Popup.tenure)+ '.', formatedTotalAmount:round(Popup.ta) + '.' } }),totalAmount.hide, totalemi.hide" />
                        </div>
                        <div class="clear"></div>
                    </div>

                    <div class="border-solid-top padding-top20 padding-bottom15">
                        <p class="grid-9 alpha text-slate-grey omega">Total amount (Principal + Interest)</p>
                        <div class="emi-slider-box-right-section grid-3 omega alpha">
                            <span class="bwmsprite inr-xxsm-icon margin-right5"></span><span id="totalAmount" class="margin-right5 text-bold">2,01,169</span>
                            <span class="hide" [class]="((Popup.formatedTotalAmount.lastIndexOf('.'))>5) ? 'text-bold': 'hide'" [text]="Popup.formatedTotalAmount.lastIndexOf('.')-5 <=1? Popup.formatedTotalAmount.substr(Popup.formatedTotalAmount.lastIndexOf('.')-6, 1)+',' : Popup.formatedTotalAmount.substr(Popup.formatedTotalAmount.lastIndexOf('.')-7, 2)+','">0,</span>
                            <span class="hide" [class]="((Popup.formatedTotalAmount.lastIndexOf('.'))>3) ? 'text-bold': 'hide'" [text]="Popup.formatedTotalAmount.lastIndexOf('.')-3 <=1? Popup.formatedTotalAmount.substr(Popup.formatedTotalAmount.lastIndexOf('.')-4, 1) : Popup.formatedTotalAmount.substr(Popup.formatedTotalAmount.lastIndexOf('.')-5, 2)">0</span>
                            <span id="1" class="hide" [class]="((Popup.formatedTotalAmount.lastIndexOf('.')) > 3) ? 'text-bold': 'hide'" [text]="Popup.formatedTotalAmount.lastIndexOf('.')>3? ',' + Popup.formatedTotalAmount.substr(Popup.formatedTotalAmount.lastIndexOf('.')-3,3) : Popup.formatedTotalAmount.substr(Popup.formatedTotalAmount.lastIndexOf('.')-3,3)">0</span>
                        </div>
                        <div class="clear"></div>
                    </div>
                    <p class="grid-5 text-default alpha position-rel pos-top2">Indicative EMI</p>
                    <div class="text-right grid-7 omega font16 text-bold">
                        <span> &#x20b9;</span>&nbsp;<span id="totalemi" class="margin-right5">5,055</span>
                        <span class="hide" [class]="((Popup.emi.lastIndexOf('.'))>5) ? 'text-bold': 'hide'" [text]="Popup.emi.lastIndexOf('.')-5 <=1? Popup.emi.substr(Popup.emi.lastIndexOf('.')-6, 1)+',' : Popup.emi.substr(Popup.emi.lastIndexOf('.')-7, 2)+','">0,</span>
                        <span class="hide" [class]="((Popup.emi.lastIndexOf('.'))>3) ? 'text-bold': 'hide'" [text]="Popup.emi.lastIndexOf('.')-3 <=1? Popup.emi.substr(Popup.emi.lastIndexOf('.')-4, 1) : Popup.emi.substr(Popup.emi.lastIndexOf('.')-5, 2)">0</span>
                        <span id="1" class="hide" [class]="((Popup.emi.lastIndexOf('.')) > 3) ? 'text-bold': 'hide'" [text]="Popup.emi.lastIndexOf('.')>3? ',' + Popup.emi.substr(Popup.emi.lastIndexOf('.')-3,3) : Popup.emi.substr(Popup.emi.lastIndexOf('.')-3,3)">0</span>
                        /month
                    </div>
                    <div class="clear"></div>
                </div>
            </div>
            <!-- model version prices start -->
            if (Model.IsNew && (Model.VersionSpecs != null || Model.HasNearestPriceCities))
            {
                <div id="versionContent" class="bw-model-tabs-data card-bottom-margin">
                    @if (Model.VersionSpecs != null)
                    {
                        <div class="content-box-shadow card-bottom-margin content-inner-block-20">
                            <h2 class="card-h2-heading margin-bottom20">@Model.BikeName price in @Model.CityEntity.CityName</h2>
                            <table class="padding-bottom10" width="100%" cellspacing="0" cellpadding="0">
                                <thead class="version-table__head">
                                    <tr class="table">
                                        <th align="left" width="30%">Variant</th>
                                        <th align="left" width="35%">Price</th>
                                        <th align="left" width="35%">Specs</th>
                                    </tr>
                                </thead>
                                <tbody class="version-table__body">
                                    @foreach (var version in Model.VersionSpecs)
                                    {
                                    <tr>
                                        <td valign="top">@version.VersionName</td>
                                        <td valign="top" class="text-default">&#x20b9;<span class="text-bold">&nbsp;@Bikewale.Utility.Format.FormatPrice(Convert.ToString(version.Price))</span></td>
                                        <td valign="top">@string.Format(@"{0} Brake, {1} Wheels", version.BrakeType, (version.AlloyWheels) ? "Alloy" : "Spoke")</td>
                                    </tr>
                                    }
                                </tbody>
                            </table>
                            <div class="padding-top10 font12 text-light-grey">Above mentioned prices are On-road, @Model.CityEntity.CityName</div>
                        </div>
                    }
                    <!-- model prices in nearby cities start -->
                    @if (Model.HasNearestPriceCities)
                    {
                        var priceQuotesCities = Model.NearestPriceCities.PriceQuoteList;
                        var top4Cities = priceQuotesCities.Take(4);
                        Model.NearestPriceCities.PriceQuoteList = top4Cities;

                        <div class="padding-top20 padding-bottom10 content-box-shadow card-bottom-margin padding-left20 padding-right20">
                            <h2 class="card-h2-heading margin-bottom10">@Model.BikeModel.ModelName price in cities near @Model.CityEntity.CityName<span class="font14 text-slate-grey text-unbold">&nbsp;(On-road price)</span></h2>
                            <table width="100%" border="0" cellspacing="0" cellpadding="0" class="font14">
                                <tbody>
