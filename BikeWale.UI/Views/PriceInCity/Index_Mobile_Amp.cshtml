                }
                <!-- model version prices end -->
                <!-- model prices similar bikes start -->
                @if (Model.HasAlternateBikes)
                {
                    <div id="similarContent" class="bw-model-tabs-data content-box-shadow padding-top20 padding-bottom5 card-bottom-margin">
                        <h2 class="card-h2-heading margin-bottom10 padding-left20 padding-right20">@string.Format("Prices of similar {0} in {1}", Model.BodyStyleText, Model.CityEntity.CityName)</h2>
                        @Html.Partial("~/views/Bikemodels/_SimilarBikes_Mobile_AMP.cshtml", Model.AlternateBikes)
                    </div>
                }
                else if (Model.IsPopularBodyStyleAvailable)
                {
                    <div id="popularContent" class="bw-model-tabs-data padding-top15 padding-bottom15 content-box-shadow card-bottom-margin">
                        <div class="carousel-heading-content">
                            <div class="swiper-heading-left-grid inline-block">
                                <h2 class="card-h2-heading padding-right20">Popular @Model.BodyStyleText</h2>
                            </div>
                            <div class="swiper-heading-right-grid inline-block text-right">
                                <a href="@String.Format("{0}/m{1}", BWConfiguration.Instance.BwHostUrl, UrlFormatter.FormatGenericPageUrl(Model.BodyStyle))" title="Best @Model.BodyStyleText in India" class="btn view-all-target-btn">View all</a>
                            </div>
                        </div>
                        @Html.Partial("~/Views/BestBikes/_PopularBodyStyleCarousel_Mobile_AMP.cshtml", Model.PopularBodyStyle)
                    </div>
                }
                <div id="modelSpecsFooter"></div>
            </div>
        </div>
    </section>

        <section>
            <div id="priceInCityWrapper" hidden>
                <div class="city-area-banner"></div>
                <div class="popup-inner-container">
                    <div class="bwmsprite onroad-price-close-btn" role="option" tabindex="0" on="tap:priceInCityWrapper.hide"></div>
                    <div id="popupHeading">
                        <p class="popupheading-head">Please Tell Us Your Location</p>
                        <div class="text-light-grey margin-bottom5">See on-road prices in your city!</div>
                        <div role="option" tabindex="1" id="pcitySelection" on="tap:priceInCityPopUpContent.show">
                            <p [text]="Popup.city">Select City</p>
                            <span class="bwmsprite fa-angle-right"></span>
                        </div>
                        <span id="priceincityerror" class="hide" [class]="Popup.cityerrorflag== 1 ?'error-text':'hide'">Please select city.</span>
                        <div id="btnPriceinCityLoader" class="center-align margin-top20 text-center position-rel">
                            <a class="hide" [class]="Popup.cityerrorflag== 0 ?'btnPriceInCityPopup':'hide'" href='#' [href]="Popup.href" role="option" tabindex="0" id="btnPriceInCityPopupURL" on="tap:AMP.setState({Popup:{cityerrorflag:(Popup.city=='Select City')? 1: 0}})">Show on-road price</a>
                            <span class="btnPriceInCityPopup" [class]="Popup.cityerrorflag != 0 ?'btnPriceInCityPopup':'hide'" role="option" tabindex="0" on="tap:AMP.setState({Popup:{cityerrorflag:(Popup.city=='Select City')? 1: 0}})">Show on-road price</span>
                        </div>
                        <div id="priceInCityPopUpContent" hidden>
                            <div class="bwm-city-area-box">
                                <div role="option" tabindex="0" class="user-input-box" on="tap:priceInCityPopUpContent.hide">
                                    <span class="back-arrow-box">
                                        <span class="bwmsprite back-long-arrow-left"></span>
                                    </span>
                                    <span id="popupPriceInCityInput">Select City</span>
                                </div>
                                <ul id="pcPopupCityList">
                                    @foreach (var City in Model.Cities)
                                    {
                                    <li role="option" tabindex="1" on="tap:priceInCityPopUpContent.hide, AMP.setState({Popup:{ city:'@City.CityName',cityerrorflag:0, href:'@Bikewale.Utility.BWConfiguration.Instance.BwHostUrl/m/honda-bikes/activa/price-in-@City.CityMaskingName/'}})">@City.CityName</li>
                                    }
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>

                <!-- model prices in other cities start -->
                if (Model.HasPriceInTopCities && Model.IsNew)
                {
                            <section>
                                <div class="container card-bottom-margin">
                                    <div class="content-box-shadow padding-bottom5 card-bottom-margin">
                                        <div class="content-inner-block-20">
                                            <h2 class="card-h2-heading margin-bottom10">@Model.BikeModel.ModelName prices in other cities</h2>
                                            <p class="font14 text-slate-grey margin-bottom15">You are currently viewing price of @Model.BikeName in @Model.CityEntity.CityName. Not from @Model.CityEntity.CityName? Select your city to see price in your city.</p>
                                            <p role="option" tabindex="0" id="changeCity" class="btn btn-white btn-170-32 changepriceincitypage bw-ga" on="tap:priceInCityWrapper.show">Select your city</p>
                                        </div>
                                        <div class="margin-right20 margin-bottom20 margin-left20 border-solid-bottom"></div>
                                        <h3 class="margin-right20 margin-bottom10 margin-left20">@Model.BikeModel.ModelName prices in popular cities</h3>
                                        <div class="swiper-container card-container swiper-city">
                                            @Html.Partial("~/views/priceincity/_PriceInTopCitiesIcon_Mobile_AMP.cshtml", Model.PriceInTopCities)
                                        </div>
                                    </div>
                                </div>
                            </section>

                <!-- model prices in other cities end -->
                }

                        if (Model.BikeRank != null && Model.BikeRank.BikeType != null)
                        {
                            <section>
                                <div class="container content-box-shadow content-inner-block-20 card-bottom-margin">
                                    <div class="margin-bottom10">
                                        <span class="bwmsprite graph-rank-grey"></span>
                                        <div class="grid-11-inline padding-left10">
                                            <h2 class="card-h2-heading">Top 10 @Model.BikeRank.StyleName in India</h2>
                                        </div>
                                    </div>
                                    <p class="font14 margin-bottom15 text-slate-grey">Are you looking for a @Model.BikeRank.BikeType.ToLower()? Check out our list of top 10 @Model.BikeRank.StyleName.ToLower() in India.</p>
                                    <a href="@String.Format("{0}/m{1}", BWConfiguration.Instance.BwHostUrl, UrlFormatter.FormatGenericPageUrl(Model.BikeRank.Rank.BodyStyle))"
                                       title="Best @Model.BikeRank.StyleName in India" class="btn view-all-target-btn">
                                        View complete list<span class="bwmsprite teal-right"></span>
                                    </a>
                                </div>
                            </section>
                        }



                        <section>
                            <div class="container card-bottom-margin padding-right20 padding-left20">
                                <p class="text-default text-bold font12 text-slate-grey">Disclaimer:</p>
                                <p class="font12 text-slate-grey">
                                    BikeWale takes utmost care in gathering precise and accurate information about @Model.BikeName price in @Model.CityEntity.CityName However, this information is only indicative and may not reflect the final price you may pay. For more information please read
                                    <a class="font12" href="@string.Format("{0}/termsconditions.aspx", BWConfiguration.Instance.BwHostUrl)" target="_blank">terms and conditions</a>,<a class="font12" href="@string.Format("{0}/visitoragreement.aspx", BWConfiguration.Instance.BwHostUrl)" target="_blank">&nbsp;visitor agreement</a>&nbsp;and&nbsp;
                                    <a class="font12" href="@string.Format("{0}/privacypolicy.aspx", BWConfiguration.Instance.BwHostUrl)" target="_blank">privacy policy</a>.
                                </p>
                            </div>
                        </section>
}


