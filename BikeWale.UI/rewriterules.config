<?xml version="1.0"?>
<rules>
  <rule name="bikewale domain redirect" stopProcessing="true">
    <match url="(.*)" ignoreCase="false" />
    <conditions logicalGrouping="MatchAll">
      <add input="{HTTP_X_FORWARDED_PROTO}" pattern="https" ignoreCase="false" negate="true" />
      <add input="{HTTP_HOST}" pattern="bikewale.com$" />
      <add input="{URL}" pattern="(web)?api(.*)$" ignoreCase="false" negate="true" />
    </conditions>
    <action type="Redirect" url="https://www.bikewale.com/{R:1}" redirectType="Permanent" />
  </rule>
  <rule name="bikewale staging domain redirect" stopProcessing="true">

    <match url="(.*)" ignoreCase="false" />
    <conditions logicalGrouping="MatchAll">
      <add input="{HTTP_X_FORWARDED_PROTO}" pattern="https" ignoreCase="false" negate="true" />
      <add input="{HTTP_HOST}" pattern="^staging.bikewale.com$" />
      <add input="{URL}" pattern="(web)?api(.*)$" ignoreCase="false" negate="true" />
    </conditions>
    <action type="Redirect" url="https://staging.bikewale.com/{R:1}" redirectType="Permanent" />
  </rule>
  <rule name="Add trailing slash" stopProcessing="true">
    <match url="(.*[^/])$" />
    <conditions>
      <add input="{REQUEST_FILENAME}" pattern="(.*?)\.(css|js|ashx|xml|cshtml|aspx|html|ascx|txt|config|jpg|png|jpeg|gif|woff|tff|svg|eof)$" negate="true" />
    </conditions>
    <action type="Redirect" redirectType="Permanent" url="{R:1}/" />
  </rule>
  <rule name="redirect for files" stopProcessing="true">
    <match url="^(([^/]+/)*([^/.]+))$" ignoreCase="false" />
    <conditions logicalGrouping="MatchAll">
      <add input="{REQUEST_FILENAME}" matchType="IsFile" ignoreCase="false" negate="true" />
      <add input="{URL}" pattern="ajax(.*)$" ignoreCase="false" negate="true" />
      <add input="{URL}" pattern="(web)?api(.*)$" ignoreCase="false" negate="true" />
      <add input="{URL}" pattern="^(?!(localhost|webserver)$).*" ignoreCase="false" negate="true" />
    </conditions>
    <action type="Redirect" url="https://{HTTP_HOST}/{R:1}" redirectType="Permanent" />
  </rule>
  <!-- Start enfield 301 -->
  <rule name="usedelectra-perm301" stopProcessing="true">
