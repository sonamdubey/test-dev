@model BikewaleOpr.Models.ManufacturerCampaign.CampaignPropertiesVM
@{
    ViewBag.Title = "Configure Campaign Properties";
    ViewBag.PageTitle = "Configure Campaign Properties for Dealer";
}

@Html.Partial("~/Views/ManufacturerCampaign/_NavigationBreadcrumbs.cshtml", 2)

<form action="/manufacturercampaign/saveproperties/" method="post" onsubmit="return Validate()">
    <div class="row">
        <div class="col s6">
            <div class="row">
                <div class="col offset-s3">
                    <input type="checkbox" class="filled-in" id="HasEmiProperties" name="HasEmiProperties" />
                    <label for="HasEmiProperties"></label>
                </div>
                <div class="col s5">
                    <span class="flow-text">Configure Emi Property</span>
                </div>
            </div>

            <div class="row">
                <div class="input-field col s4 offset-s3">
                    <input id="EmiButtonTextMobile" name="EmiButtonTextMobile" type="text" maxlength="100" disabled="disabled">
                    <label for="EmiButtonTextMobile">Button Text Mobile Site</label>
                </div>

                <div class="col">
                    <input type="checkbox" class="filled-in default-chk" id="chkEmiButtonMobile" name="chkEmiButtonMobile" checked="checked" disabled="disabled" />
                    <label for="chkEmiButtonMobile">Use Default</label>
                </div>
            </div>

            <div class="row">
                <div class="input-field col s4 offset-s3">
                    <input id="EmiPropertyTextMobile" name="EmiPropertyTextMobile" type="text" class="validate" maxlength="100" disabled="disabled">
                    <label for="EmiPropertyTextMobile">Property Text Mobile Site</label>
                </div>

                <div class="col">
                    <input type="checkbox" class="filled-in default-chk" id="chkEmiPropertyMobile" name="chkEmiPropertyMobile" checked="checked" disabled="disabled" />
                    <label for="chkEmiPropertyMobile">Use Default</label>
                </div>
            </div>

            <div class="row">
                <div class="input-field col s4 offset-s3">
                    <input id="EmiButtonTextDesktop" name="EmiButtonTextDesktop" type="text" class="validate" maxlength="100" disabled="disabled">
                    <label for="EmiButtonTextDesktop">Button Text Desktop Site</label>
                </div>

                <div class="col">
                    <input type="checkbox" class="filled-in default-chk" id="chkEmiButtonDesktop" name="chkEmiButtonDesktop" checked="checked" disabled="disabled" />
                    <label for="chkEmiButtonDesktop">Use Default</label>
                </div>
            </div>

            <div class="row">
                <div class="input-field col s4 offset-s3">
                    <input id="EmiPropertyTextDesktop" name="EmiPropertyTextDesktop" type="text" class="validate" maxlength="100" disabled="disabled">
                    <label for="EmiPropertyTextDesktop">Property Text Desktop Site</label>
                </div>

                <div class="col">
                    <input type="checkbox" class="filled-in default-chk" id="chkEmiPropertyDesktop" name="chkEmiPropertyDesktop" checked="checked" disabled="disabled" />
                    <label for="chkEmiPropertyDesktop">Use Default</label>
                </div>
            </div>

            <div class="row">
                <div class="input-field col s4 offset-s3">
                    <select id="EmiPriority" name="EmiPriority" disabled>
                        <option value="" disabled selected>Property Priority</option>
                        <option value="1">Option 1</option>
                        <option value="2">Option 2</option>
                        <option value="3">Option 3</option>
                    </select>
                </div>
            </div>

        </div>
        <div class="col s6">
            <div class="row">
                <div class="col offset-s3">
                    <input type="checkbox" class="filled-in" id="HasLeadProperties" name="HasLeadProperties" />
                    <label for="HasLeadProperties"></label>
                </div>
                <div class="col s6">
                    <span class="flow-text">Configure Leads Property</span>
                </div>
            </div>

            <div class="row">
                <div class="input-field col s4 offset-s3">
                    <input id="LeadButtonTextMobile" name="LeadButtonTextMobile" type="text" class="validate" maxlength="100" disabled="disabled">
                    <label for="LeadButtonTextMobile">Button Text Mobile Site</label>
                </div>

                <div class="col">
                    <input type="checkbox" class="filled-in default-chk" id="chkLeadsButtonMobile" name="chkLeadsButtonMobile" checked="checked" disabled="disabled" />
                    <label for="chkLeadsButtonMobile">Use Default</label>
                </div>
            </div>

            <div class="row">
                <div class="input-field col s4 offset-s3">
                    <input id="LeadPropertyTextMobile" name="LeadPropertyTextMobile" type="text" class="validate" maxlength="100" disabled="disabled">
                    <label for="LeadPropertyTextMobile">Property Text Mobile Site</label>
                </div>

                <div class="col">
                    <input type="checkbox" class="filled-in default-chk" id="chkLeadsPropertyMobile" name="chkLeadsPropertyMobile" checked="checked" disabled="disabled" />
                    <label for="chkLeadsPropertyMobile">Use Default</label>
                </div>
            </div>

            <div class="row">
                <div class="input-field col s4 offset-s3">
                    <input id="LeadButtonTextDesktop" name="LeadButtonTextDesktop" type="text" class="validate" maxlength="100" disabled="disabled">
                    <label for="LeadButtonTextDesktop">Button Text Desktop Site</label>
                </div>

                <div class="col">
                    <input type="checkbox" class="filled-in default-chk" id="chkLeadsButtonDesktop" name="chkLeadsButtonDesktop" checked="checked" disabled="disabled" />
                    <label for="chkLeadsButtonDesktop">Use Default</label>
                </div>
            </div>

            <div class="row">
                <div class="input-field col s4 offset-s3">
                    <input id="LeadPropertyTextDesktop" name="LeadPropertyTextDesktop" type="text" class="validate" maxlength="100" disabled="disabled">
                    <label for="LeadPropertyTextDesktop">Property Text Desktop Site</label>
                </div>

                <div class="col">
                    <input type="checkbox" class="filled-in default-chk" id="chkLeadsPropertyDesktop" name="chkLeadsPropertyDesktop" checked="checked" disabled="disabled" />
                    <label for="chkLeadsPropertyDesktop">Use Default</label>
                </div>
            </div>


            <div class="row">
                <div class="input-field col s4 offset-s3">
                    <input id="LeadHtmlMobile" name="LeadHtmlMobile" type="text" class="validate" maxlength="100" disabled="disabled">
                    <label for="LeadHtmlMobile">Mobile Site HTML Template</label>
                </div>

                <div class="col">
                    <input type="checkbox" class="filled-in default-chk" id="chkLeadsMobileTemplate" name="chkLeadsMobileTemplate" checked="checked" disabled="disabled" />
                    <label for="chkLeadsMobileTemplate">Use Default</label>
                </div>
            </div>

            <div class="row">
                <div class="input-field col s4 offset-s3">
                    <input id="LeadHtmlDesktop" name="LeadHtmlDesktop" type="text" class="validate" maxlength="100" disabled="disabled">
                    <label for="LeadHtmlDesktop">Desktop Site HTML Template</label>
                </div>

                <div class="col">
                    <input type="checkbox" class="filled-in default-chk" id="chkLeadsDesktopTemplate" name="chkLeadsDesktopTemplate" checked="checked" disabled="disabled" />
                    <label for="chkLeadsDesktopTemplate">Use Default</label>
                </div>
            </div>

            <div class="row">
                <div class="input-field col s4 offset-s3">
                    <select id="LeadPriority" name="LeadPriority" disabled>
                        <option value="" disabled selected>Property Priority</option>
                        <option value="1">Option 1</option>
                        <option value="2">Option 2</option>
                        <option value="3">Option 3</option>
                    </select>
                </div>
            </div>

            <div class="row">
                <div class="col offset-s3">
                    <button id="btnConfigureCampaign" name="btnConfigureCampaign" class="btn waves-effect waves-light" type="submit" name="action">
                        Configure Campaign
                    </button>
                </div>
            </div>
        </div>
    </div>

</form>

    <script src="/src/ManufactureCampaign/ConfigureCampign.js"></script>
