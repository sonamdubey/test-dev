            <add input="{HTTP_HOST}" pattern="^(opr|bwoprst).bikewale.com$" matchType="Pattern" />
          </conditions>
          <action type="Redirect" url="https://{HTTP_HOST}{R:1}" redirectType="Permanent" />
        </rule>
      </rules>
    </rewrite>
