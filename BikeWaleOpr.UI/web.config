<configuration>
  <configSections>
    <section name="memcached" type="Enyim.Caching.Configuration.MemcachedClientSection, Enyim.Caching"/>
  </configSections>
  <system.web>
    <authentication mode="Forms">
      <forms name="_bikewaleOpr" loginUrl="http://localhost:83"/>
    </authentication>    
    <httpRuntime requestValidationMode="2.0" />
    <compilation debug="true"/>
  </system.web>
  <connectionStrings>
    <add name="RabbitMQServerList" connectionString="192.168.1.220"/>
  </connectionStrings>
  <appSettings>
