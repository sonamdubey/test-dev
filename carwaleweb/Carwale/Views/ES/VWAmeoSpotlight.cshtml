<script>
    var VolkswagenAmeo = {
        doc: $(document),
        personLeadCity: {},
        newWidth: null,
        apiInput: {},

        compareCarWithVolkswagenAmeo: function () {

        },

        responsive: {

            responsiveCarousel: function () {
                var jcarousel = $('.ameo-carousel .jcarousel');
                jcarousel
                    .on('jcarousel:reload jcarousel:create', function () {
                        var carousel = $(this);
                        if (VolkswagenAmeo.newWidth == null) {
                            VolkswagenAmeo.newWidth = carousel.innerWidth();
                        }
                        // carousel.jcarousel('items').css('width', Math.ceil(VolkswagenAmeo.newWidth)/3 + 'px');
                    })
                    .jcarousel();
            },

            registerEvent: function () {

                $('.full-width-carousel .jcarousel').jcarousel('scroll', 0);
                $(".full-width-carousel .jcarousel ul li").css("width", $(window).outerWidth());
                VolkswagenAmeo.responsive.responsiveCarousel();
                $('.ameo-carousel .jcarousel').jcarousel('scroll', 0);

                VolkswagenAmeo.newWidth = null;
               
                $(".content-parent").css({ 'width': $(".ameo-img-box").width() - 1 });
                if ($(document).width() <= 622) {
                    $(".content-parent").css({ 'height': 'auto' });
                } else {
                    $(".content-parent").css({ 'height': $(".ameo-img-box").height() });
                }
                for (var i = 0; i < $(".content-parent").length; i++) {
                    if ($(".content-parent").get(i).scrollHeight > $(".content-parent").height()) {
                        $($(".ameo-content-box")[i]).addClass("zero-transform");
                    } else {
                        $($(".ameo-content-box")[i]).removeClass("zero-transform");
                    }
                }
            }
        },

        lazyImg: {

            callEventLazy: function () {
                $("img.lazy").lazyload();
                $(window).load(function () {
                    VolkswagenAmeo.lazyImg.applyLazyLoad();
                });
            },

            applyLazyLoad: function () {
                $("img.lazy").lazyload({
                    event: "imgLazyLoad",
                    effect: "fadeIn"
                });
            }
        },

        leadForm: {

            registerEvent: function () {

                $("#esLeadFormSubmit").click(function () {
                    VolkswagenAmeo.leadForm.submit($(this));
                });

                if (typeof isMobileDevice == "undefined") {
                    $("#personCity").cw_autocomplete({
                        resultCount: 5,
                        source: ac_Source.allCarCities,
                        click: function (event, ui, orgTxt) {
                            VolkswagenAmeo.personLeadCity.name = Common.utils.getSplitCityName(ui.item.label);
                            VolkswagenAmeo.personLeadCity.id = ui.item.id;
                            ui.item.value = VolkswagenAmeo.personLeadCity.name;
                        },
                        open: function (result) {
                            VolkswagenAmeo.personLeadCity.result = result;
                        },
                        afterfetch: function (result, searchtext) {
                            this.result = result;
                            if (typeof result == "undefined" || result.length <= 0)
                                VolkswagenAmeo.leadForm.showHideMatchError(true, $('#personCity'), "No city Match");
                            else
                                VolkswagenAmeo.leadForm.showHideMatchError(false, $('#personCity'));
                        }
                    });
                }
                else {
                    var personCityInputField = $("#personCity");

                    $(personCityInputField).cw_easyAutocomplete({
                        inputField: personCityInputField,
                        resultCount: 5,
                        source: ac_Source.allCarCities,
                        click: function (event) {
                            var selectionValue = personCityInputField.getSelectedItemData().value,
                            selectionLabel = personCityInputField.getSelectedItemData().label;

                            VolkswagenAmeo.personLeadCity.name = Common.utils.getSplitCityName(selectionLabel);
                            VolkswagenAmeo.personLeadCity.id = selectionValue;
                            $(personCityInputField).val(VolkswagenAmeo.personLeadCity.name);
                        },

                        afterFetch: function (result, searchText) {
                            VolkswagenAmeo.personLeadCity.result = result;

                            if (typeof result == "undefined" || result.length <= 0) {
                                VolkswagenAmeo.leadForm.showHideMatchError(true, $(personCityInputField).closest('.easy-autocomplete'), "No city Match");
                            }
                            else {
                                VolkswagenAmeo.leadForm.showHideMatchError(false, $(personCityInputField).closest('.easy-autocomplete'));
                            }
                        }
                    });
                }
            },

            submit: function (event) {
                var nameEle = $(event).parent().find("#personName");
                //var emailEle = $(event).parent().find("#personEmail");
                var mobileEle = $(event).parent().find("#personMob");
                var cityEle = $(event).parent().find("#personCity");
                var err = form.validation.contact(nameEle.val(), "ameo@carwale.com", mobileEle.val()); //, $.trim(cityEle.val())); emailEle.val(), 

                if (VolkswagenAmeo.leadForm.processErrorResults(err, nameEle, mobileEle, cityEle)) { //emailEle,
                    $("#esLeadFormSubmit").html("Submitting...").attr("disabled", true);
                    VolkswagenAmeo.apiCall.leadPush(VolkswagenAmeo.leadForm.getUserObject(nameEle.val(), mobileEle.val()), VolkswagenAmeo.leadForm.processResponse); //emailEle.val(), 
                }
            },
            getUserObject: function (custname, custmobile) {
                var customerinfo = {
                    carName: carName,
                    name: custname,
                    mobile: custmobile,
                    cityid: VolkswagenAmeo.personLeadCity.id,
                    versionId: "",
                    modelId: modelid,
                    makeId: "",
                    leadType: PushleadType,
                    cityName: VolkswagenAmeo.personLeadCity.name
                };
                return customerinfo;
            },
            processResponse: function (data) {
                $("#es-leadform").hide();
                $("#es-thankyou").show();
                $(".request-callback-box .grid-4").css("background", "white");
            },
            validateCity: function (targetId) {

                var cityVal = Common.utils.getSplitCityName(targetId.val());
                if (cityVal == $.cookie("_CustCityMaster") && typeof (VolkswagenAmeo.personLeadCity) != "undefined" && Number(VolkswagenAmeo.personLeadCity.id) > 0 && VolkswagenAmeo.personLeadCity.id == $.cookie("_CustCityIdMaster")) {
                    if (typeof isMobileDevice == "undefined") {
                        VolkswagenAmeo.leadForm.showHideMatchError(false, targetId);
                    }
                    else {
                        VolkswagenAmeo.leadForm.showHideMatchError(false, $(targetId).closest('.easy-autocomplete'));
                    }
                    return true;
                }
                else if (cityVal == "" || targetId.hasClass('border-red') ||
                            (
                                ($('li.ui-menu-item a:visible:eq(0)').text() != cityVal && cityVal != "") &&
                                (typeof (VolkswagenAmeo.personLeadCity) == "undefined" || typeof (VolkswagenAmeo.personLeadCity.name) == "undefined" || VolkswagenAmeo.personLeadCity.name.toLowerCase() != cityVal.toLowerCase())
                            )
                       ) {
                    if (typeof isMobileDevice == "undefined") {
                        VolkswagenAmeo.leadForm.showHideMatchError(true, targetId, "Please Enter City");
                    }
                    else {
                        VolkswagenAmeo.leadForm.showHideMatchError(true, $(targetId).closest('.easy-autocomplete'), "Please Enter City");
                    }
                    return false;
                }
                return true;
            },
            processErrorResults: function (err, nameEle, mobileEle, cityEle) { //emailEle,

                var isFormValid = true;
                if (err && err.length > 2) {

                    if (err[0] == "") {
                        nameEle.siblings().addClass('hide');
                        nameEle.removeClass('border-red');
                    }
                    else {
                        nameEle.siblings().removeClass('hide');
                        nameEle.addClass('border-red');
                        var errSpan = nameEle.siblings()[1];
                        $(errSpan).text(err[0]);
                        isFormValid = false;
                    }

                    if (err[2] == "") {
                        mobileEle.siblings('.cw-blackbg-tooltip, .error-icon').addClass('hide');
                        mobileEle.removeClass('border-red');
                    }
                    else {
                        mobileEle.siblings('.cw-blackbg-tooltip, .error-icon').removeClass('hide');
                        mobileEle.addClass('border-red');
                        var errSpan = mobileEle.siblings()[1];
                        $(errSpan).text(err[2]);
                        isFormValid = false;
                    }
                }
                if (!VolkswagenAmeo.leadForm.validateCity(cityEle)) {
                    isFormValid = false;
                }
                return isFormValid;
            },
            showHideMatchError: function (error, TargetId, errText) {
                if (error) {
                    TargetId.siblings('.error-icon').removeClass('hide');
                    TargetId.siblings('.cw-blackbg-tooltip').removeClass('hide').text(errText);
                    TargetId.addClass('border-red');
                }
                else {
                    TargetId.siblings().addClass('hide');
                    TargetId.removeClass('border-red');
                }
            }

        },

        apiCall: {

            leadPush: function (customerinfo, callback) {
                var url = window.location.href.toLowerCase();
                VolkswagenAmeo.apiInput.DealerId = 6898;
                VolkswagenAmeo.apiInput.CityId = customerinfo.cityid;
                VolkswagenAmeo.apiInput.ActualDealerId = 21975;
                VolkswagenAmeo.apiInput.ModelId = customerinfo.modelId;
                VolkswagenAmeo.apiInput.Email = '';
                VolkswagenAmeo.apiInput.Name = customerinfo.name;
                VolkswagenAmeo.apiInput.Mobile = customerinfo.mobile;
                VolkswagenAmeo.apiInput.BuyTimeText = "1 week";
                VolkswagenAmeo.apiInput.BuyTimeValue = 7;
                VolkswagenAmeo.apiInput.RequestType = 1;
                VolkswagenAmeo.apiInput.UtmaCookie = isCookieExists('__utma') ? $.cookie('__utma') : '';
                VolkswagenAmeo.apiInput.UtmzCookie = isCookieExists('_cwutmz') ? $.cookie('_cwutmz') : '';
                VolkswagenAmeo.apiInput.InquirySourceId = 191;
                VolkswagenAmeo.apiInput.LeadClickSource = 247;
                VolkswagenAmeo.apiInput.PlatformSourceId = 1;
                VolkswagenAmeo.apiInput.PQId = 0;
                VolkswagenAmeo.apiInput.ModelsHistory = '';
                VolkswagenAmeo.apiInput.IsAutoApproved = false;
                VolkswagenAmeo.apiInput.AssignedDealerId = -1;
                VolkswagenAmeo.apiInput.SponsoredBannerCookie = '';
                VolkswagenAmeo.apiInput.Comments = url.indexOf("source") > 0 ? Common.utils.getValueFromQS('source') : "";

                $.ajax({
                    type: 'POST',
                    url: '/webapi/DealerSponsoredAd/PostDealerInquiry/',
                    data: VolkswagenAmeo.apiInput,
                    success: function (response) {
                        callback(response);
                    },
                    error: function () {
                        callback(null);
                    }
                });
            }
        },
        isScrolledIntoView: function (elem) {
            var docViewTop = $(window).scrollTop();
            var docViewBottom = docViewTop + $(window).height();

            var elemTop = $(elem).offset().top;
            var elemBottom = elemTop + $(elem).height();

            return ((elemBottom <= docViewBottom) && (elemTop >= docViewTop));
        },
        openPqWidget: function (versionId, modelId, pageId) {
            var caption = "";
            var url = "/new/quickpqwidget.aspx?model=" + modelId + "&version=" + versionId + "&pageid=" + pageId;
            var applyIframe = false;
            var GB_Html = "";
            GB_show(caption, url, 230, 500, applyIframe, GB_Html);
        }
    }

