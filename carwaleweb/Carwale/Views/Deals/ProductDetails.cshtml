@model Carwale.DTOs.Deals.ProductDetailsDTO_Desktop
@using Carwale.UI.PresentationLogic;
@using Carwale.Utility;
@{
    ViewBag.PageTag = "A-1";
    ViewBag.Title = "CarWale Advantage";
    ViewBag.landingPage = false;
    ViewBag.Description = "CarWale is India's most authoritative source of new car pricing. Focused around car buyers, CarWale promises to help you buy the right car at the right price.";
    ViewBag.Keywords = "";
    ViewBag.BodyClass = "bg-white header-fixed-inner";
    ViewBag.Canonical = "";
    ViewBag.Alternate = "";
    ViewBag.BodyClass = "header-fixed-inner";
    ViewBag.CustomCss = "~/Views/StaticPartials/Deals/DealsCss.cshtml";
    ViewBag.CustomJs = "~/Views/StaticPartials/Deals/DealsScript.cshtml";
    ViewBag.PageId = (int)Carwale.Entity.Enum.CwPages.AdvantageProductDetailsDesktop;
}

@section MetaTags {
    <meta name="robots" content="noindex" />
}

<div class="blackOut-window pd-blackwindow" style="display:block;"></div>

<div id="loadingCarImg" style="display:block;">
    <div class="loading-popup">
        <span class="loading-icon"></span>
        <div class="clear"></div>
    </div>
</div>

<div class="globalcity-popup rounded-corner2 bg-light-grey tran-ease-out-all hide" id="advantage-detail-popup">

    <div class="advantage-close-btn position-abt pos-top10 pos-right10 cwsprite cross-lg-lgt-grey cur-pointer z-index1"></div>
    <div id="detailPopup" class="position-abt pos-left0 pos-top0">
        <div class="advantage-left-popup bg-white padding-left15 padding-right15 padding-top30 padding-bottom20 text-center">
            <div id="divSavings" class="hide">
                <div class="text-center" data-bind="attr:{stock:stockId}">
                    <h2 class="text-black font28 text-bold">₹ <span id="popupSavings"></span>/- <span class="price-off text-grey font25">off</span></h2>
                    <div class="text-dark-grey font17 margin-top10 text-bold margin-bottom5">on <span id="popupCarName"></span></div>
                    <div class="text-red font14 margin-bottom20" id="popupHurryText">Hurry only one car left</div>
                    <div>
                        <div class="font14 text-gray text-bold">100% refund policy</div>
                        <div class="text-light-gray font13 margin-top5 margin-bottom15">Get 100% refund of online booking amount if you change your mind for any reason</div>
                    </div>
                    <div class="text-dark-gray text-bold margin-bottom30">Genuine Authorized Dealers only</div>
                </div>
                <div class="margin-bottom10 text-center">
                    <span class="desk-deals-car-sprite book-now-thumb"></span>
                    <span class="desk-deals-car-sprite advantage-icon"></span>
                </div>
            </div>
            <div id="whatWeGet" class="clear form-helper-content hide">
                <div class="margin-left10">
                    <h3 class="margin-bottom30 font20 margin-right40">What You Get</h3>
                    <ul class="font14 text-left">
                        <li class="margin-bottom20 text-black">
                            <span class="fa fa-phone margin-right5 text-light-gray"></span>
                            <span class="what-you-get-text">A phone call from one of our executives</span>
                        </li>
                        <li class="margin-bottom20 text-black">
                            <span class="fa fa-usd margin-right5 text-light-gray"></span>
                            <span class="what-you-get-text">EMI, T-permit and price breakup information</span>
                        </li>
                        <li class="margin-bottom10 text-black">
                            <span class="fa fa-user margin-right5 text-light-gray"></span>
                            <span class="what-you-get-text">Scheduled dealer visit for a test drive</span>
                        </li>
                    </ul>
                </div>
            </div>

            <div class="clear"></div>
        </div>

        <div class="advantage-right-popup padding-left20 padding-top30">
            <h3 class="form-heading font20 margin-bottom30 text-capitalize text-black">Contact Details</h3>
            <div class="contact-det-popup-content">
                <div class="form-control-box margin-bottom20">
                    <input type="text" name="personName" placeholder="Name" id="personName" class="form-control hide" maxlength="50" value="unknown" />
                    <span class="cwsprite error-icon hide"></span>
                    <div class="cw-blackbg-tooltip global-city-error-msg hide">Please enter Name</div>
                </div>
                <div class="form-control-box margin-bottom20">
                    <span class="position-abt pos-right15 pos-top15 cwmsprite cross-sm-dark-grey cur-pointer"></span>
                    <input type="text" name="personEmail" placeholder="Email" id="personEmail" class="form-control hide" maxlength="100" value="unknown@unknown.com" />
                    <input type="text" class="form-control hide" id="personEmail">
                    <span class="cwsprite error-icon hide"></span>
                    <div class="cw-blackbg-tooltip hide">Please enter email id</div>
                </div>
                <div class="form-control-box margin-bottom15">
                    <span class="form-mobile-prefix">+91</span>
                    <input type="tel" name="personMob" placeholder="Mobile No." id="personMob" class="form-control padding-left40" maxlength="10" autofocus />
                    <span class="cwsprite error-icon hide"></span>
                    <div class="cw-blackbg-tooltip hide">Please enter mobile number</div>
                </div>
                <div class="form-control-box margin-bottom20">
                    <p class="text-black font14 margin-bottom5">When do you plan to purchase your car?</p>
                    <select class="form-control drpPurchaseTimeframe" id="drpPurchaseTimeframe">
                        <option value="0" disabled selected="selected">Select</option>
                        <option value="1">Immediately</option>
                        <option value="2">Within 2 weeks</option>
                        <option value="3">Within a month</option>
                        <option value="4">Next few months</option>
                    </select>
                </div>

                <div>
                    <button class="btn btn-orange font18 text-uppercase hide" type="button" id="proceedBtn"
                            data-role="click-tracking" data-event="Advantage" data-action="deals_clickinterestLink" data-cat="deals_desktop" data-label="proceedBtn">
                        SHOW FINAL OFFER
                    </button>
                    <button class="btn btn-orange font18 text-uppercase hide" type="button" id="btnAssistance">
                        REQUEST ASSISTANCE
                    </button>
                    <div id="nextBtnProcessing">
                        <img src="https://imgd.aeplcdn.com/0x0/statics/loader.gif" width="16" height="16" style="position: relative; top: 4px;" />&nbsp;&nbsp;Loading....
                    </div>
                </div>
            </div>
            <div class="form-helper-content margin-top15">
                <ul class="margin-bottom10 font14 text-left">
                    <li class="margin-bottom20 text-black">
                        <span class="fa fa-lock margin-right5 text-light-gray"></span>
                        <span class="what-you-get-text">We respect your privacy. Your contact details are safe with us.</span>
                    </li>
                </ul>
            </div>

            <div class="thank-you-screen text-center hide">
                <h3 class="font16 margin-bottom10 text-capitalize text-black">Thank You !</h3>
                <span class="cwm-md-circle md-green-thanks-icon"></span>
                <p class="text-light-grey font13 border-solid-bottom padding-bottom20">You would soon get a call from our business executive to help you.</p>
                <p class="padding-top15 text-black font14 padding-left10 padding-right10"><span class="text-bold">Hurry!</span> Reserve your car online and make sure no one else gets it.</p>
            </div>

        </div>
    </div>
    <!-- /detailPopup -->
    <div class="clear"></div>

</div>

<div class="globalcity-popup new-advantage-popup rounded-corner2 bg-white tran-ease-out-all hide" id="enquire-detail-popup">
    <div class="enquiry-popup-close-btn globalcity-close-btn position-abt pos-top10 pos-right10 cwsprite cross-md-dark-grey cur-pointer"></div>
    <div class="enquiry-poup-container ">
        <div class="enquiry-first-screen">
            <div class="left-enquiry-form">
                <div class="enquire-step1">
                    <h2 class="font18 text-bold" >Enquire for <span id="selectedCarInfo"></span> <span class="text-light-grey font12">(<span id="popupColorName"></span>)</span></h2>
                    <div class="check-options margin-top20 margin-bottom20">
                        <div class="margin-right10">
                            <label class="font14"><input type="checkbox" id="chkColorFlexible" /> I am flexible with my choice of colour</label>
                        </div>
                        @*<div class="margin-right10">
                            <label class="font14"><input type="checkbox" /> I'm ok with other variants</label>
                        </div>*@
                    </div>
                    <div class="enquiry-form-container">
                        <div class="form-control-box margin-bottom20">
                            <select class="form-control drpPurchaseTimeframe" id="drpPurchaseTime">
                                <option value="0" disabled="" selected="selected">When do you plan to purchase your car?</option>
                                <option value="1">Immediately</option>
                                <option value="2">Within 2 weeks</option>
                                <option value="3">Within a month</option>
                                <option value="4">Next few months</option>
                            </select>
                        </div>
                        <div class="form-control-box margin-bottom15">
                            <span class="form-mobile-prefix">+91</span>
                            <input type="tel" name="personMob" placeholder="Mobile No." id="personMobEnquiryPopup" class="form-control padding-left40" maxlength="10" autofocus="">
                            <span class="cwsprite error-icon hide"></span>
                            <div class="cw-blackbg-tooltip hide">Please enter mobile number</div>
                        </div>
                        <div class="enquiry-button-control margin-top20 margin-bottom10 block">
                            <a href="javascript:void(0)" id="submit-request" class="btn btn-orange btn-sm font16 cur-pointer text-uppercase margin-right10">Get Offer</a>
                            <div class="inline-block call-query">
                                <p class="font13 text-grey margin-bottom5">
                                    For any queries, call CarWale at
                                    <span class="font16 inline-block toll-call">
                                        <span class="fa fa-phone text-green"></span>
                                        <span class="font16">1800 209 0230</span>
                                    </span>
                                    <span>&nbsp;(Toll free)</span>
                                </p>
                            </div>
                            <p id="popupDealerLocation" class="margin-top10 text-light-grey font12"></p>
                            <p class="margin-top10"><span class="fa fa-lock margin-right5"></span> We respect your privacy. Your contact details are safe with us. </p>
                            <p class="margin-top10">Read complete <a href="/advantage/faq/" target="_blank" data-role="click-tracking" data-event="CWInteractive" data-cat="deals_desktop"
                                                                     data-action="popup2_faq_clicked" data-label="desktop">FAQs</a> here</p>
                        </div>
                    </div>
                </div>
            </div>
           
            <div class="left-enquiry-form-2 hide">
                <div class="enquire-thanks-container text-center">
                    <div class="thanks-top-content">
                        <h2 class="font18 text-bold margin-bottom20">Thank You</h2>
                        <span class="cw-circle-icon green-thanks-icon"></span>
                        <p class="font14">You would soon get a call  from our business executive to help you with your requested queries.</p>
                    </div>
                </div>
            </div>
        </div>
        <div class="enquiry-second-screen">
            <div class="right-enquiry-form">
                <div class="right-enquiry-step1">
                    <div class="car-interest contentWrapper position-rel margin-top20">
                        <h3 class="text-grey text-bold font16">Your car of interest</h3>
                        <div class="imageWrapper" id="interestCarImgDiv">
                            <img id="interestCarImg" alt="" title="" src="">
                        </div>
                        <div class="content-inner-block-10 car-detail" id="carDetailPopup" data-bind="with: stock">
                            <h4 class="font14 text-unbold"><span class="car-model-name text-default"><span id="interestedCarName"></span></span></h4>
                            <div class="font13 margin-top5 margin-bottom5" id="interestedCarPrices">
                                <span class="reducedTxt position-rel text-black">
                                    ₹ <span id="OnRoadAmount"></span>
                                </span>
                                <span class="text-green">(₹ <span id="SavingsAmount"></span> off)</span>
                            </div>
                            <div class="font16 text-black">
                                <p class="text-bold">₹ <span id="OfferAmount"></span></p>
                                <p class="text-grey font12">On-Road Price <span id="interestedCarCityName"></span></p>
                            </div>

                        </div>
                    </div>
                    <div class="clear wht-you-get-content margin-top20">
                        <div class="margin-left10">
                            <h3 class="text-grey font16 text-bold margin-bottom15">What you get</h3>
                            <ul class="font14 text-left what-get-list-container">
                                <li class="margin-bottom20 text-black">
                                    <span class="fa fa-user margin-right5 text-light-gray"></span>
                                    <div class="what-get-list-detail">
                                        <span class="what-you-get-title text-grey">Connect with authorized dealer</span>
                                        <p>Get all your queries solved by our executives</p>
                                    </div>
                                </li>
                                <li class="margin-bottom20 text-black">
                                    <span class="fa fa-inr margin-right5 text-light-gray"></span>
                                    <div class="what-get-list-detail">
                                        <span class="what-you-get-title text-grey">EMI, price breakup and T-permit information</span>
                                        <p>Get to know all details in one call</p>
                                    </div>
                                </li>
                                <li class="margin-bottom10 text-black">
                                    <span class="fa fa-calendar margin-right5 text-light-gray"></span>
                                    <div class="what-get-list-detail">
                                        <span class="what-you-get-title text-grey">Schedule a dealer visit and get a test drive</span>
                                        <p>Get your car delivered on priority</p>
                                    </div>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
            <div class="right-enquiry-form-2 hide">

                <div class="clear"></div>
                <div class="right-enquiry-recommendation pos-relative">
                    <div class="bg-white recommendedCarsDetail" id="recommendedCarsDetail">
                        <div class="content-inner-block-15 recommend-container">
                            <h3 class="margin-bottom10 font18 text-black">Other cars you may be interested in</h3>
                            <p class="font12">Here are some other car offers we think you might be interested in. Select the ones in which you are interested to get further assistance.</p>

                            <div id="selectRecommendation" class="inline-block">
                                <p class="text-link font14 select-all toggle-selection-view">
                                    <span class="inline-block valign-middle text-bold toggle-selection-txt">Select All</span>
                                    <span class="select-bar inline-block valign-middle">
                                        <span class="circle-icon"><img src="@( Carwale.Utility.CWConfiguration._imgHostUrl)0x0/cw/design15/toggle-slider-circle.png"></span>
                                    </span>
                                </p>
                            </div>
                            <div class="jcarousel-wrapper">
                                <div class="jcarousel">
                                    <ul class="list-rcmd-cars" data-bind="foreach: deals">
                                        <li class="contentWrapper content-box-shadow position-rel cur-pointer rounded-corner2" data-bind="attr:{'data-stockid': $data.stockId , 'data-dealerid' : $data.dealerId}">
                                            <span class="cust-checkbox position-abt pos-right5 pos-top5 fa fa-square-o"></span>
                                            <div class="imageWrapper">
                                                <img width="230" height="130" alt="" title="" data-bind="attr: { 'src': $data.carImgDetails.hostUrl + '310x174' + $data.carImgDetails.originalImgPath }">
                                            </div>
                                            <div class="content-inner-block-10">
                                                <h4 class="font14 text-unbold">
                                                    <a class="car-model-name text-default" target="_blank" data-bind="attr: { 'href': '/' + Common.utils.formatSpecial($data.carMake.makeName) + '-cars/' + $data.carModel.maskingName + '/advantage/?src=d20&cityid=' + $data.cityId }"
                                                       data-role="click-tracking" data-event="CWInteractive" data-cat="deals_desktop" data-action="dealsaccess_desktop" data-label="recommendation_popup2">
                                                        <span data-bind="text: $data.carMake.makeName + ' ' + Common.utils.filterModelName($data.carModel.modelName)"></span>
                                                    </a>
                                                </h4>
                                                <div class="font13 margin-top5 margin-bottom5">
                                                    <span class="reducedTxt position-rel text-black">
                                                        ₹ <span data-bind="text: Common.utils.formatNumeric($data.onRoadPrice)"></span>
                                                    </span>
                                                    <!-- ko 'if':$data.savings > 0 -->
                                                    <span class="text-green">(₹ <span data-bind="text: Common.utils.formatNumeric($data.savings)"></span> off)</span>
                                                    <!-- /ko -->
                                                    <!-- ko 'if':$data.savings == 0 -->
                                                    <span class="text-green">&nbsp;Offer Available</span>
                                                    <!-- /ko -->
                                                </div>
                                                <div class="font16 text-black">
                                                    <p class="text-bold">₹ <span data-bind="text: Common.utils.formatNumeric($data.offerPrice)"></span></p>
                                                    <p class="text-grey font13" data-bind="text: 'On-Road Price ' + $data.cityName"></p>
                                                </div>
                                                <!-- ko 'if':$data.stockCount == 1 -->
                                                <p class="margin-top5 font12"><span data-bind="text: $data.stockCount"></span> car available</p>
                                                <!-- /ko -->
                                                <!-- ko 'if':$data.stockCount > 1 && $data.stockCount <= 3 -->
                                                <p class="margin-top5 font12"><span data-bind="text: $data.stockCount"></span> cars available</p>
                                                <!-- /ko -->
                                            </div>
                                        </li>
                                    </ul>
                                </div>
                                    <span class="jcarousel-control-left recommendationCarousel"><span class="cwsprite jcarousel-control-prev"></span></span>
                                    <span class="jcarousel-control-right recommendationCarousel"><span class="cwsprite jcarousel-control-next"></span></span>
                            </div>

                            <div class="clear"></div>
                            <div class="error margin-top10 hide">
                                *Please select a car
                            </div>
                            <input class="btn btn-orange btn-md margin-top10 text-uppercase margin-bottom5" id="multiLeadPush" type="button" value="request assistance" />

                        </div>
                    </div>
                    <div class="clear"></div>
                </div>
                <div class="clear"></div>
            </div>
        </div>
        <div class="clear"></div>
    </div>
</div>
<div class="clear"></div>

<!-- Make Model PopUp Code Starts Here -->
<div class="make-model-popup rounded-corner2 hide" id="make-model-popup">
    <!-- global city pop up code starts here -->
    <div class="globalcity-popup-data text-center">
        <div class="make-model-close-btn globalcity-close-btn position-abt pos-top10 pos-right10 cwsprite cross-lg-lgt-grey cur-pointer"></div>

        <h3 class="font20 margin-bottom15 text-capitalize text-black">Change Car</h3>

        <select class="form-control selected-text selectCity margin-top5" id="drpSelectDealMake" data-bind="options: makes, optionsValue:$data, optionsText: 'makeName' ,optionsCaption:'Select a Make',value:selectedMake,event:{change:updateModels}"></select>
        <select class="form-control selected-text selectCity margin-top20 margin-bottom20" id="drpSelectDealModel" data-bind="options: models, optionsValue:$data, optionsText: 'modelName' ,optionsCaption:'Select a Model',value:selectedModel"></select>
        <div>
            <button id="btnConfirmCity" class="btn btn-orange btn-sm font16 text-uppercase margin-right10" type="button" data-src="d12" data-bind="event:{click:doneClick}">Done </button>
            <a href="/advantage/?cityId=@ViewBag.CityId" class="btn btn-xs btn-white font16 text-uppercase" id="viewMoreCars" data-role="click-tracking" data-event="CWInteractive" data-action="landingPageAccess_Desktop" data-cat="deals_desktop" data-label="ChangeCarPopUp">View All Cars (@Model.DealsCount) </a>
        </div>
    </div>
    <div class="clear"></div>
</div>
<!-- Make Model PopUp Code Ends Here -->
<div class="min-height">
