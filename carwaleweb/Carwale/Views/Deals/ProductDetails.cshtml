                                            <a href="javascript:void(0)" class="changeCarPopup" data-role="click-tracking" data-event="Advantage" data-action="carchangelinkaccess_desktop" data-cat="deals_desktop" data-label="car-change-link">
                                                Looking for a different car?
                                            </a>
                                        </div>*@
                                    <!-- Advantage Landing page Link Ends Here-->

                                    <div data-bind="foreach:carColorDetails">
