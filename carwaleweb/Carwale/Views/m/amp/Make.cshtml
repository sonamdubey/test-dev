<aside class="adblock">
    <div id="top_banner_ad" class="adblock-content">
        <amp-ad width=320 height=50 type="doubleclick" data-slot="/1017752/CarWale_Mobile_ROS_320x50">
            <div placeholder></div>
        </amp-ad>
    </div>
</aside>
<main role="main">
    @if (Model.NewCarModels.Count > 0)
    {
    <h1>@Model.MakeDetails.MakeName Cars</h1>

        <hr /> 
    <ul class="model__list">
        @foreach (var item in Model.NewCarModels)
        {
        <li>
            @if (!item.New)
            {
                <div class="upcoming-label">
                    <span class="modelType">Upcoming</span>
                </div>
            }
      @if (item.ShowDate)
         {
            <div class="just-launched-section">
                <div class="launch-title">
                    @if (item.New)
                    {
                        @Html.Raw("LAUNCHED ON");
                    }
                    else
                    {
                        @Html.Raw("EXPECTED TO LAUNCH ON");
                    }
                </div>
                <div class="launch-date-unit">
                    <span class="launch-date">@item.LaunchDate.Day<span class="lauch-date-unit">@Format.GetNumberSuffix(item.LaunchDate.Day)</span></span><br>
                    <span class="launch-month">@item.LaunchDate.ToString("MMM", CultureInfo.InvariantCulture).ToUpper()</span>
                </div>
            </div>
          }
            <a title="@Model.MakeDetails.MakeName @item.ModelName"
               data-vars-event-label="amp-@item.ModelName"
               href='@ManageCarUrl.CreateModelUrl(Model.MakeDetails.MakeName,item.MaskingName)'>
                <div>
                    @if (!string.IsNullOrEmpty(item.OriginalImage))
                    {
                    <amp-img src="@ImageSizes.CreateImageUrl(item.HostUrl,ImageSizes._272X153, item.OriginalImage)"
                             width="227"
                             height="128"
                             layout="responsive">
                    </amp-img>
                    }
                    else
                    {
                    <amp-img src="https://imgd.aeplcdn.com/272X153/cw/cars/no-cars.jpg" title="@Model.MakeDetails.MakeName @item.ModelName" alt="@Model.MakeDetails.MakeName @item.ModelName"
                             width="227"
                             height="128"
                             layout="responsive">
                    </amp-img>
                    }
                </div>
            </a>
            <div class="model__card">

                <a title="@Model.MakeDetails.MakeName @item.ModelName"
                   data-vars-event-label="amp-@item.ModelName"
                   href='@ManageCarUrl.CreateModelUrl(Model.MakeDetails.MakeName,item.MaskingName)'>
                    <h2>@item.ModelName</h2>
                </a>
                @if (item.CarPriceOverview != null)
                { 
                <div class="margin-bottom5">
                    <b>@item.CarPriceOverview.PricePrefix <span>@item.CarPriceOverview.Price</span> @item.CarPriceOverview.PriceSuffix</b>
                    @if (CookiesCustomers.MasterCityId <= 0)
                    {
                    <a data-vars-event-label="amp-@item.ModelName-showpic" href="@ManageCarUrl.CreateModelUrl(Model.MakeDetails.MakeName,item.MaskingName)?q=globalcity" class="show-city-price text-center" tabindex="0">Show Price in my city</a>
                    }
                    <br />
                    @Html.Raw(item.CarPriceOverview.PriceLabel)@(!string.IsNullOrEmpty(item.CarPriceOverview.City.Name) ? ", " + item.CarPriceOverview.City.Name : string.Empty)
                </div>
                }
                @if (!item.New && !item.ShowDate)
                {
                    @Html.Raw(ConfigurationManager.AppSettings["MakePageUpcomingMonthText"] + " <span class='text-bold'>" + item.LaunchDate.ToString("MMMM yyyy") + "</span>");
                }
                @if (item.ReviewCount > 0)
                {
                <div class="rating-sprite @CommonOpn.GetRateImage(item.ModelRating)"></div><span> @item.ReviewCount User Reviews</span>
                }
            </div>

            @if (item.Rank > 0)
            {
            <div class="model__rank__slug">
                <div class="position-abt text-center">
                    <span class="list-count text-lighter-grey"><span>@item.Rank</span></span>
                    <div class="triangle triangle-left"></div>
                    <div class="triangle triangle-right"></div>
                </div>
                <div class="position-rel pos-left10">
                    <p><b>No.@item.Rank in Top@(item.NoOfTopCarsInBodyType.Equals(10) ? " 10" : string.Empty) @(item.BodyStyleId.ToFriendlyString() + "s")</b></p>
                    <a data-vars-event-label="amp-@(item.BodyStyleId.ToFriendlyString() + "s")-@Model.MakeDetails.MakeName" title="@HtmlAttributes.GetTopBodyTypeLinkTitle(item.NoOfTopCarsInBodyType, item.BodyStyleId)" href="@ManageCarUrl.CreateTopCarsByBodyTypeUrl(item.BodyStyleId.ToUrlString())">View Complete List</a>
                </div>
            </div>
            }


        </li>
        }
    </ul>
    }
        @{Html.RenderAction("Description", "ModelComponent", new { makeId = Model.MakeDetails.MakeId, makeName = Model.MakeDetails.MakeName, isAmp = true });}
    <section id="news" class="alpha omega">
        @{Html.RenderAction("TopLatestNewsForMake", "ModelComponent", new { makeId = Model.MakeDetails.MakeId, makeName = Model.MakeDetails.MakeName, isAmp = true });}
    </section>
    <section id="othermakes" class="bg-white content-box-shadow">
        @{Html.RenderAction("AllMakesWidget", "CarWidgets", new { excludeMakeId = Model.MakeDetails.MakeId, isMobile = false, isAmp = true });}
    </section>
