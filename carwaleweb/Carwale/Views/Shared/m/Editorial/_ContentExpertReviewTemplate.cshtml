<script type="text/html" id="expertReviewsTemplate">
    <span data-bind="attr:{id : 'dfp_' + basicId}"></span>
    <div class="scroll-container" data-bind="attr:{divbasicId:basicId}">
        <div class="inner-container">
            <div class="margin-top10 margin-bottom10 padding-left15 padding-right15">
                <div>
                    <h1 class="margin-top10 margin-bottom10 m-special-skin-text" data-bind="text:title"></h1>
                </div>
                <div class="floatleft margin-right20 m-special-skin-text">
                    <span class="blue">
                        <span data-bind="if : authorMaskingName != null && authorMaskingName != '' ">
                            <span data-bind="text : Editorial.utils.getDisplayDate(publishedDate) + ' IST by ' "></span><a data-bind="attr:{href : '/m/authors/' + authorMaskingName + '/'}"><span data-bind="text :  authorName"></span></a>
                        </span>
                        <span data-bind="if : authorMaskingName == null || authorMaskingName == ''">
                            <span data-bind="text: Editorial.utils.getDisplayDate(publishedDate) + ' IST by ' + authorName"></span>
                        </span>
                    </span>
                </div>
                <div class="line-border margin-top3"></div>
            </div>
            <div class="clear"></div>
            <div class="article-reading-content bg-white">
                <div class="content-inner-block-15 margin-bottom10 rounded-corner2 content-details related-details" data-bind="attr : {id : basicId,categoryid : categoryId,categoryname : 'expert-reviews'}">
                    <div data-bind="foreach:pageList">
                        <h2 class="darkgray" data-bind="text:pageName"></h2>
                        <div class="line-border margin-top5"></div>
                        <div class="darkgray content-font" data-bind="html:content"></div>
                    </div>
                </div>
                <div class="social light-shadow padding-bottom15">
                    <a data-bind="attr:{href:'https://m.facebook.com/sharer.php?u=https://carwale.com' + articleUrl + '?utm_source=share&t=' + title, 'data-label':'https://www.carwale.com/m' + articleUrl}"
                       data-role="click-tracking" data-event="CWInteractive" data-action="Facebook" data-cat="share" target='_blank'>
                        <div id="FbCount" class="contentSocial facebook margin-bottom10 margin-left15"></div>
                    </a>
                    <a data-bind="attr:{href:'https://twitter.com/share?url=https://www.carwale.com' + articleUrl + '?utm_source=share&t=' + title, 'data-label':'https://www.carwale.com/m' + articleUrl}" target='_blank' style='text-decoration:none;'
                       data-role="click-tracking" data-event="CWInteractive" data-action="Twitter" data-cat="share">
                        <div id="TweetCount" class="contentSocial twitter margin-bottom10"></div>
                    </a>
                    <a data-bind="attr:{href:'mailto:contact@carwale.com?subject=' + title + '&body=I was reading https://www.carwale.com' + articleUrl + '?utm_source=share on CarWale site and thought to share with you.', 'data-label':'https://www.carwale.com/m' + articleUrl}"
                       data-role="click-tracking" data-event="CWInteractive" data-action="Email" data-cat="share">
                        <div class="contentSocial email margin-bottom10"></div>
                    </a>
                    <a data-bind="attr:{href:'whatsapp://send?text=https://www.carwale.com'+articleUrl+'?utm_source=share', 'data-label':'https://www.carwale.com/m'+articleUrl}"
                       data-role="click-tracking" data-event="CWInteractive" data-action="Whatsapp" data-cat="share">
                        <div class="contentSocial whatsapp margin-bottom10"></div>
                    </a>
                    <div class="clear"></div>
                </div>
            </div>
        </div>
    </div>
</script>


