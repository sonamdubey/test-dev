    <div class="container">
        <div id="newsContainer" class="grid-12 alpha omega gridView">
            <!-- Left container code starts here-->
            <div class="grid-8 left-container alpha omega">
                <ul id="viewUl" data-bind="template:{name:'liTemplate',foreach:current()}" class="main-frame leftfloat position-rel">
                    @if (Model.Page1 != null)
                    {
                        var index = 0;
                        foreach (var item in Model.Page1.Articles)
                        {
                            <li class="news-box" data-id="@(Model.ExpertCategories.Contains(item.CategoryId)?"expert-reviews":(Model.FeatureCategories.Contains(item.CategoryId)?"features":"news"))">
                                <div class="news-inner-wrap position-rel">
                                    <div class="news-inner-box">
                                        <div style="@(index++ < 2 ? string.Format("background-image:url('{0}');background-repeat:NO-REPEAT;background-size:cover;", Carwale.Utility.ImageSizes.CreateImageUrl(item.HostUrl,"310x174",item.OriginalImgUrl)) : string.Empty)" class="img-wrap min-height-170">
                                            <a class="news-title text-black" href="@item.ArticleUrl" rel="bookmark">
                                                <img class="lazy min-height-170" src="https://imgd.aeplcdn.com/0x0/statics/grey.gif" data-original="@Carwale.Utility.ImageSizes.CreateImageUrl(item.HostUrl,"310x174",item.OriginalImgUrl)" />
                                            </a>
                                        </div>
                                        <div class="desc-wrap content-inner-block-10">
                                            <span class="ed-tag font12 text-uppercase text-white text-center">@item.CategoryMaskingName</span>
                                            <h2 class="font16 news-title">
                                                <a class="news-title text-black" href="@item.ArticleUrl" rel="bookmark">@item.Title</a>
                                            </h2>
                                            <div class="font12 margin-top10 margin-bottom5">
                                                <time datetime="@item.FormattedDisplayDate" itemprop="datePublished"></time>
                                                <abbr>@item.FormattedDisplayDate</abbr>&nbsp;by&nbsp;
                                                @if (string.IsNullOrWhiteSpace(item.AuthorMaskingName))
                                                {
                                                    <span itemprop="author">@item.AuthorName</span>
                                                }
                                                else
                                                {
                                                    <a href="/authors/@item.AuthorMaskingName/"><span itemprop="author">@item.AuthorName</span></a>
                                                }
                                            </div>
                                            <div class="desc-content hide">
                                                @Html.Raw(item.Description)
                                            </div>
                                        </div>
                                    </div>
                                    <p class="postmetadata padding-right10 padding-bottom10 text-right text-default font14 hide">
                                        <span class="fa fa-eye"></span> <span class="font12">@item.Views views</span>
                                        <meta content="UserPageVisits:@item.Views" itemprop="interactionCount" />
                                    </p>
                                </div>
                            </li>
                        }
                    }
                    else if (Model.MediaPage1 != null)
                    {
                        var index = 0;
                        if (Model.MediaPage1.Photos != null)
                        {
                            foreach (var item in Model.MediaPage1.Photos.PhotosList)
                            {
                                <li class="news-box news-view-box news-hover-list" data-id="photos">
                                    <div class="news-inner-wrap position-rel">
                                        <div class="news-inner-box">
                                            <div class="img-wrap">

                                                @if (ViewBag.isMobile)
                                                {
                                                    <div style="@(index++ < 2 ? string.Format("background-image:url('{0}');background-repeat:NO-REPEAT;background-size:cover;", Carwale.Utility.ImageSizes.CreateImageUrl(item.HostURL,"310x174",item.OriginalImgPath)) : string.Empty)" class="min-height-170">
                                                        <a class="news-title text-black" href="/m/@UrlRewrite.FormatSpecial(item.MakeName)-cars/@item.ModelMaskingName/images/">
                                                            <img class="lazy min-height-170" src="@( Carwale.Utility.CWConfiguration._imgHostUrl)0x0/cw/design15/grey.gif" data-original="@Carwale.Utility.ImageSizes.CreateImageUrl(item.HostURL,"310x174",item.OriginalImgPath)" />
                                                        </a>
                                                    </div>
                                                }
                                                else
                                                {
                                                    <a class="news-title text-black" href="@("/" + UrlRewrite.FormatSpecial(item.MakeName) + "-cars/" + item.ModelMaskingName + "/images/")">
                                                        <img class="lazy" src="@( Carwale.Utility.CWConfiguration._imgHostUrl)0x0/cw/design15/grey.gif" data-original="@Carwale.Utility.ImageSizes.CreateImageUrl(item.HostURL,"310x174",item.OriginalImgPath)" />
                                                    </a>
                                                }

                                            </div>
                                            <div class="view-wrap content-inner-block-10">
                                                <span class="image-tag font14 text-uppercase text-white text-center"><span class="fa fa-camera"></span>@item.ImageCount</span>
                                                <h2 class="font16 news-title margin-top15">
                                                    @if (ViewBag.isMobile)
                                                    {
                                                        <a class="news-title text-black" href="/m/@UrlRewrite.FormatSpecial(item.MakeName)-cars/@item.ModelMaskingName/images/" rel="bookmark">@item.MakeName @item.ModelName</a>
                                                    }
                                                    else
                                                    {
                                                        <a class="news-title text-black" href="@("/" + UrlRewrite.FormatSpecial(item.MakeName) + "-cars/" + item.ModelMaskingName + "/images/")" rel="bookmark">@item.MakeName @item.ModelName</a>
                                                    }
                                                </h2>
                                            </div>
                                        </div>

                                    </div>
                                </li>
                            }
                        }
                        else if (Model.MediaPage1.Videos != null)
                        {

                            foreach (var item in Model.MediaPage1.Videos.VideosList)
                            {
                                var videoUrl = CMSCommon.GetVideoUrl(item.MakeName, item.ModelMaskingName, item.Title, item.BasicId, ViewBag.isMobile);
                                
                                <li class="news-box news-view-box news-hover-list" data-id="videos">
                                    <div class="news-inner-wrap position-rel">
                                        <div class="news-inner-box">
                                            <div class="img-wrap position-rel">

                                                @if (ViewBag.isMobile)
                                                {
                                                    <div style="@(index++ < 2 ? string.Format("background-image:url('https://img.youtube.com/vi/{0}/mqdefault.jpg');background-repeat:NO-REPEAT;background-size:cover;", item.VideoId) : string.Empty)" class="min-height-170">
                                                        <a class="news-title text-black" href="@videoUrl">
                                                            <img class="lazy min-height-170" src="@( Carwale.Utility.CWConfiguration._imgHostUrl)0x0/cw/design15/grey.gif" data-original="https://img.youtube.com/vi/@item.VideoId/mqdefault.jpg" />
                                                        </a>
                                                    </div>
                                                }
                                                else
                                                {
                                                    <a class="news-title text-black" href="@videoUrl">
                                                        <img class="lazy" src="@( Carwale.Utility.CWConfiguration._imgHostUrl)0x0/cw/design15/grey.gif" data-original="https://img.youtube.com/vi/@item.VideoId/mqdefault.jpg" />
                                                        <div class="center-container hide"><span class="carousel-youtube-video-icon"></span></div>
                                                        <div class="swiper-overlay-bg hide"></div>
                                                    </a>
                                                }

                                            </div>
                                            <div class="view-wrap content-inner-block-10">
                                                
                                                <h2 class="font16 news-title margin-top15">
                                                    @if (ViewBag.isMobile)
                                                    {
                                                        <a class="news-title text-black" href="@videoUrl" rel="bookmark">@item.Title</a>
                                                    }
                                                    else
                                                    {
                                                        <a class="news-title text-black" href="@videoUrl" rel="bookmark">@item.Title</a>
                                                    }
                                                </h2>
                                            </div>
                                        </div>

                                    </div>
                                </li>
                            }


                        }
                    }
                </ul>

                <script type="text/html" id="liTemplate">
                    <!-- ko if : $parent.currentTabNonComputed!= "images" && $parent.currentTabNonComputed!= "videos"-->
                    <li class="news-box" data-bind="attr:{'data-id':$parent.getCategory(CategoryId)}">
                        <div class="" data-bind="attr:{'class':'news-inner-wrap position-rel article-read-'+BasicId}">
                            <div class="news-inner-box">
                                <div class="img-wrap">
                                    <a class="text-black" rel="bookmark" data-bind='attr:{"href":(window.isMobile?"/m":"")+ArticleUrl}'>
                                        <img class="lazy min-height-170" src="@( Carwale.Utility.CWConfiguration._imgHostUrl)0x0/cw/design15/grey.gif" data-bind="attr:{'data-original': HostUrl+'310x174'+ (OriginalImgUrl.indexOf('?')>0?OriginalImgUrl.replace('?','?q=85&'):OriginalImgUrl+'?q=85')}" />
                                    </a>
                                </div>
                                <div class="desc-wrap content-inner-block-10">
                                    <span class="ed-tag font12 text-uppercase text-white text-center" data-bind="text:(CategoryMaskingName.toLowerCase()=='features'?'Special Report':CategoryMaskingName),style:{visibility:$parent.getVisibility($data)}"></span>
                                    <h2 class="font16 news-title">
                                        <a class="text-black" rel="bookmark" data-bind='text:Title,attr:{"href":(window.isMobile?"/m":"")+ArticleUrl}'></a>
                                    </h2>
                                    <div class="font12 margin-top10 margin-bottom5 author-data">
                                        <abbr data-bind='text:FormattedDisplayDate'></abbr>&nbsp;by&nbsp;
                                        <!-- ko if : $.trim(AuthorMaskingName) != ""-->
                                        <a data-bind="attr:{'href':(window.isMobile?'/m':'')+'/authors/'+ AuthorMaskingName + '/'}"><span itemprop="author" data-bind="text:AuthorName"></span></a>
                                        <!-- /ko -->
                                        <!-- ko if : $.trim(AuthorMaskingName) == ""-->
                                        <span itemprop="author" data-bind="text:AuthorName"></span>
                                        <!-- /ko -->
                                    </div>
                                    <div class="untrimmed"><p class="desc-content hide" data-bind="html:Description"></p></div>
                                    <div class="trimmed"><p class="desc-content hide" data-bind="html:Description"></p></div>
                                </div>
                            </div>
                        </div>
                    </li>
                    <!-- /ko -->
                    <!-- ko if : $parent.currentTabNonComputed == "images" -->
                    <li class="news-box news-view-box news-hover-list">
                        <div class="news-inner-wrap position-rel">
                            <div class="news-inner-box">
                                <div class="img-wrap">
                                    <!-- ko ifnot : window.isMobile -->
                                    <a class="news-title text-black" data-bind="attr: {'href': $parent.imageUrl($data)}">
                                        <img class="lazy" src="@( Carwale.Utility.CWConfiguration._imgHostUrl)0x0/cw/design15/grey.gif" data-bind="attr:{'data-original': hostUrl+'310x174'+ originalImgPath,'alt':makeName+' '+modelName +' Pictures','title':makeName+' '+modelName+' Images'}" onerror="this.onerror=null;this.src='@( Carwale.Utility.CWConfiguration._imgHostUrl)310x174/cw/cars/no-cars.jpg';" />
                                    </a>
                                    <!-- /ko -->
                                    <!-- ko if : window.isMobile -->
                                    <a class="news-title text-black" data-bind="attr:{'href': $parent.imageUrl($data)}">
                                        <img class="lazy min-height-170" src="@( Carwale.Utility.CWConfiguration._imgHostUrl)0x0/cw/design15/grey.gif" data-bind="attr:{'data-original': hostUrl+'310x174'+ (originalImgPath.indexOf('?')>0?originalImgPath.replace('?','?q=85&'):originalImgPath+'?q=85'),'alt':makeName+' '+modelName +' Pictures','title':makeName+' '+modelName+' Images'}" onerror="this.onerror=null;this.src='@( Carwale.Utility.CWConfiguration._imgHostUrl)310x174/cw/cars/no-cars.jpg';" />
                                    </a>
                                    <!-- /ko -->
                                </div>
                                <div class="view-wrap content-inner-block-10">
                                    <!-- ko if : is360ExteriorAvailable || is360InteriorAvailable || is360OpenAvailable --><a data-role="click-tracking" data-event="CWInteractive" data-action="content_landing" data-bind="attr:{'href':$parent.get360Url($data),'data-cat':(window.isMobile?'msite_360_linkages':'desktop_360_linkages'),'data-label':(makeName+' '+modelName)}"><span data-role="inview-imp" data-action="content_landing" data-bind="attr:{'data-cat':(window.isMobile?'msite_360_linkages_impression':'desktop_360_linkages_impression'),'data-label':(makeName+' '+modelName)}" class="three60-tag font14 text-uppercase text-white text-center">360° VIEW</span></a><!-- /ko -->
                                    <a data-bind="attr:{'href': $parent.imageUrl($data)}"><span class="image-tag font14 text-uppercase text-white text-center"><span class="fa fa-camera"></span><span data-bind="text: imageCount"></span></span></a>
                                    <h2 class="font16 news-title margin-top15">
                                        <!-- ko ifnot : window.isMobile -->
                                        <a class="news-title text-black" data-bind="attr: {'href': $parent.imageUrl($data)}, text: makeName + ' ' + modelName" rel="bookmark"></a>
                                        <!-- /ko -->
                                        <!-- ko if : window.isMobile -->
                                        <a class="news-title text-black" data-bind="attr:{'href':$parent.imageUrl($data)},text: makeName+' '+modelName" rel="bookmark"></a>
                                        <!-- /ko -->
                                    </h2>
                                </div>
                            </div>

                        </div>
                    </li>
                    <!-- /ko -->
                    <!-- ko if : $parent.currentTabNonComputed == "videos" -->
                    <li class="news-box news-view-box news-hover-list">
                        <div class="news-inner-wrap position-rel">
                            <div class="news-inner-box">
                                <div class="img-wrap position-rel">
                                    <!-- ko ifnot : window.isMobile -->
                                    <a class="news-title text-black" data-bind="attr:{'href': $parent.videoUrl($data) ,'data-role': 'click-tracking' ,'data-event': 'CWInteractive' ,'data-cat': 'desktop_video_linkages' ,'data-label': makeName + ' ' + modelName ,'data-action': 'content_landing'}">
                                        <img class="lazy" src="@( Carwale.Utility.CWConfiguration._imgHostUrl)0x0/cw/design15/grey.gif" data-bind="attr:{'data-original': 'https://img.youtube.com/vi/'+videoId+'/mqdefault.jpg','alt':makeName+' '+modelName +' Videos','title':makeName+' '+modelName+' Videos'}" onerror="this.onerror=null;this.src='@( Carwale.Utility.CWConfiguration._imgHostUrl)310x174/cw/cars/no-cars.jpg';" />
                                        <div class="center-container hide"><span class="carousel-youtube-video-icon"></span></div>
                                        <div class="swiper-overlay-bg hide"></div>
                                    </a>
                                    <!-- /ko -->
                                    <!-- ko if : window.isMobile -->
                                    <a data-role="click-tracking" data-event="CWInteractive" data-action="video_card" data-cat="msite_content_landing" class="news-title text-black" data-bind="attr:{'data-label' : makeName+' '+modelName, 'href': $parent.videoUrl($data)}">
                                        <img class="lazy min-height-170" src="@( Carwale.Utility.CWConfiguration._imgHostUrl)0x0/cw/design15/grey.gif" data-bind="attr:{'data-original': 'https://img.youtube.com/vi/'+videoId+'/mqdefault.jpg','alt':makeName+' '+modelName +' Videos','title':makeName+' '+modelName+' Videos'}" onerror="this.onerror=null;this.src='@( Carwale.Utility.CWConfiguration._imgHostUrl)310x174/cw/cars/no-cars.jpg';" />
                                        
                                    </a>
                                    <!-- /ko -->
                                </div>
                                <div class="view-wrap content-inner-block-10">
                                    
                                    <h2 class="font16 news-title margin-top15">
                                        <!-- ko ifnot : window.isMobile -->
                                        <a class="news-title text-black" rel="bookmark" data-bind="text: title,attr:{'href': $parent.videoUrl($data),'data-role': 'click-tracking' ,'data-event': 'CWInteractive' ,'data-cat': 'desktop_video_linkages' ,'data-label': makeName + ' ' + modelName ,'data-action': 'content_landing'}"></a>
                                        <!-- /ko -->
                                        <!-- ko if : window.isMobile -->
                                        <a data-role="click-tracking" data-event="CWInteractive" data-action="video_card" data-cat="msite_content_landing" class="news-title text-black" rel="bookmark" data-bind="text: title,attr:{'data-label' : makeName+' '+modelName, 'href': $parent.videoUrl($data)}"></a>
                                        <!-- /ko -->
                                    </h2>
                                </div>
                            </div>

                        </div>
                    </li>
                    <!-- /ko -->
                </script>

                <script type="text/html" id="makeBreadCrumb">
                    <!-- ko if : $parent.make() != null -->
                    <li><span class="fa fa-angle-right margin-right10"></span><a data-bind="attr:{href: (window.isMobile? '/m/':'/')+name+'-cars/'},text:displayName"></a></li>
                    <!-- /ko -->
                </script>
                <script type="text/html" id="modelBreadCrumb">
                    <!-- ko if : $parent.model() != null -->
                    <li><span class="fa fa-angle-right margin-right10"></span><a data-bind="attr:{href:'/'+$parent.make().name+'-cars/'+name+'/'},text:displayName"></a></li>
                    <!-- /ko -->
                </script>
                <div class="clear"></div>
            </div>
            <!-- Left container code ends here-->

        </div>


        <div class="clear"></div>
        <script type="text/html" id="paginationTemplate">
            <!--ko if :$data>0 && $parent.uiPageNumber()-1>0-->
            <li><a data-bind="attr: {'href': ($parent.model() != null ? '/' + $parent.make().name + '-cars' + '/' + $parent.model().name : $parent.make() != null ? '/' + $parent.make().name + '-cars' : '') + ('/'+($parent.currentTab() == 'all' ? 'news' : $parent.currentTab()) +'/page/' +($parent.uiPageNumber()-1)+'/' )}">Prev</a></li>
            <!--/ko-->
            <!--ko if :$data!=0 && $data!=-2-->
            <li><a data-bind="text:$data,attr: {'href': ($parent.model() != null ? '/' + $parent.make().name + '-cars' + '/' + $parent.model().name : $parent.make() != null ? '/' + $parent.make().name + '-cars' : '') + '/'+($parent.currentTab() == 'all' ? 'news' : $parent.currentTab())+'/' +($data==1? '' : ('page/'+$data+'/')) ,'class':($parent.uiPageNumber()==$data?'active':'')}"></a></li>
            <!--/ko-->
            <!--ko if :$data==-2-->
            <li><a data-bind="attr: {'href': ($parent.model() != null ? '/' + $parent.make().name + '-cars' + '/' + $parent.model().name : $parent.make() != null ? '/' + $parent.make().name + '-cars' : '') + ('/'+($parent.currentTab() == 'all' ? 'news' : $parent.currentTab())+'/page/' +($parent.uiPageNumber()+1)+'/' )}">Next</a></li>
            <!--/ko-->
        </script>
        <div class="footerStrip" id="divStrip">
            <div class="text-center padding-top15 margin-auto block news-pagination hide">
                <ul class="pagination bg-white" data-bind="template:{name:'paginationTemplate',foreach:pagination}">
                    @if (Model.HasPrevPage)
                    {
                        <li><a href="@(EditorialContent.GetListingPaginationUrl(Model.pageNumber - 1, ViewBag.IsMobile, Model))">Prev</a></li>
                    }
                    @foreach (var i in Model.Pages)
                    {
                        <li><a href="@(EditorialContent.GetListingPaginationUrl(i, ViewBag.IsMobile, Model))" @Html.Raw(Model.pageNumber == i ? "class=\"active\"" : string.Empty)>@i</a></li>
                    }
                    @if (Model.HasNextPage)
                    {
                        <li><a href="@(EditorialContent.GetListingPaginationUrl(Model.pageNumber + 1, ViewBag.IsMobile, Model))">Next</a></li>
                    }
                </ul>
            </div>
        </div>
    </div>
    <div class="cwmsprite scrollToTop scroll-top cd-top cur-pointer" id="back-to-top"></div>
</section>