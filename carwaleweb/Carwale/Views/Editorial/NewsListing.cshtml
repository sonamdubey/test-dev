                <li data-bind="css:{'active':currentTab()=='all','disabled':allCount()<1}" data-tabs="all">News <span data-bind="text:allCount,css:{'hide': !filterApplied()}" class="filter-news-count hide">0</span></li>
                <li data-bind="css:{'active':currentTab()=='expert-reviews','disabled':expertReviewsCount()<1}" data-tabs="expert-reviews">Reviews<span data-bind="text:expertReviewsCount,css:{'hide': !filterApplied()}" class="filter-news-count hide">0</span></li>
                <li data-bind="css:{'active':currentTab()=='features','disabled':featuresCount()<1}" data-tabs="features">Special Reports <span data-bind=" text:featuresCount,css:{'hide': !filterApplied()}" class="filter-news-count hide">0</span></li>
