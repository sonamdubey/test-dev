@model Carwale.DTOs.Classified.CarDetails.CarDetailsModel
<script type="text/javascript">
    var videoUrl = '@(String.IsNullOrEmpty(Model.carDetailEntity.BasicCarInfo.VideoURL) || Model.carDetailEntity.BasicCarInfo.IsPremium == false || Model.carDetailEntity.BasicCarInfo.VideoCount == 0 ? "" : ("https://www.youtube.com/embed/" + Model.carDetailEntity.BasicCarInfo.VideoURL))';
    var profileId = '@ViewBag.profileId';
    var inquiryId = '@ViewBag.sellInqId';
    var _isDealer = '@(ViewBag.sellerId == 1?"1":"0")';
    var consumerType = _isDealer == "1" ? "1" : "2";
    var carName = '@(Model.carDetailEntity.BasicCarInfo.MakeName + " " + Model.carDetailEntity.BasicCarInfo.ModelName + " " + Model.carDetailEntity.BasicCarInfo.VersionName)';
    var ImageName = '';
    var MainCategory = '';
    var MakeId = '@Model.carDetailEntity.BasicCarInfo.MakeId';
    var ModelId = '@Model.carDetailEntity.BasicCarInfo.ModelId';
    var MakeName = '@Model.carDetailEntity.BasicCarInfo.MakeName';
    var ModelName = '@Model.carDetailEntity.BasicCarInfo.ModelName';
    var VersionName = '@Model.carDetailEntity.BasicCarInfo.VersionName';
    var cityId= '@Model.carDetailEntity.BasicCarInfo.CityId';
    var cityName='@Model.carDetailEntity.BasicCarInfo.CityName';
    var price ='@Model.carDetailEntity.BasicCarInfo.Price';
    var dealerText = _isDealer =="1" ? "Dealer" : "";
    var individualText = _isDealer == "0" ? "Individual" : "";
    var MakeYear = '@Model.carDetailEntity.BasicCarInfo.MakeYear.ToString("yyyy")';
</script>

