@model Carwale.DTOs.NewCars.ModelPageDTO_Desktop
@using Carwale.Utility;
@using System.Configuration;
<script type="text/javascript">
    window.ISVERSIONPAGE = false;
    var isModelPage = "True";
    var dealerPopupSourceId = 5, leadsSubmitted = 0;
    var LeadClickSource = 102, InquirySourceId = 99;
    var isHashHaveModel = false;
    var usedLuxuryCount = '@(Model.UsedLuxuryCars?.Count ?? 0)';
    var modelId = '@Model.ModelDetails.ModelId';
    var makeId = '@Model.ModelDetails.MakeId';
    var IsOverviewPage = false;
    var PageId = '@(Model.OverviewDetails?.PageId ?? 0)';
    var priceQuoteClicked = false;
    var modelName = '@Model.ModelDetails.ModelName';
    var subscriptionCategory = 1;
    var subscriptionType = @Model.ModelDetails.ModelId;
    var makename = '@Model.ModelDetails.MakeName';
    var VersionId = '@((Model.NewCarVersions != null && Model.NewCarVersions.Count > 0) ? Model.OverviewDetails?.VersionId ?? 0 : 0)';
    var videoSlider = 1;
    var CityId = '@ViewBag.CityId' ;
    var CityName = '@ViewBag.CityName';
    var isCityPage = @(ViewBag.IsCityPage ? "true" : "false");
    var SubSegmentId = @Model.ModelDetails.SubSegmentId;
    var newCarString = '@Model.ModelDetails.New'.toLowerCase();
    var newCar = (newCarString === "true")
    var maskingName = '@Model.ModelDetails.MaskingName';
    var zoneNameForModelVersion = '@ViewBag.CityZone';
    var carModelName = '@ViewBag.CarModelName';
    var isPriceShown = '@ViewBag.IsPriceShown';
    var USERIP = "@Carwale.Utility.UserTracker.GetUserIp()";
</script>
@if (Model.ModelDetails.New)
{
    <script type="text/javascript">var product = {identifier: '@Model.ModelDetails.ModelId' };</script>
    if (HttpContext.Current.User.Identity.IsAuthenticated)
    {
        <script type="text/javascript">var customer = {mhash: '@Carwale.UI.Common.CurrentUser.Email.ComputeHash(Hasher.eHashType.SHA256)'};</script>
    }
}
<script type="text/javascript" src="https://www.youtube.com/iframe_api" defer></script>
<script type="text/javascript" src="/static/js/videostracking.js" defer></script>
<script type="text/javascript" src="/static/src/pq-common.js" defer></script>
<script type="text/javascript" src="/static/src/dropdown-menu.min.js" defer></script>
<script type="text/javascript" src="/static/js/newcarsmodel.js" defer></script>
<script type="text/javascript" src="/static/js/model-version-common.js" defer></script>
<script type="text/javascript" src="~/static/js/editorial-common.js" defer></script>
<script type="text/javascript" src="/static/js/lead-form-support.js" defer></script>

<script type="text/javascript" src="/static/js/utils/dropdown.js" defer></script>

@if (Model.ModelDetails.Futuristic &&  Model.ModelDetails.ModelId == CWConfiguration.MahindraMozarroModelid)
{
<script type="text/javascript" src="/static/js/leadform-v1.js" defer></script>
<script>
    isMobile = false;
    PushleadType = 28; 
    modelid = @Model.ModelDetails.ModelId;
    carName = '@Model.ModelDetails.MakeName'+' '+'@Model.ModelDetails.ModelName';
</script>
}
@if (Model.ModelDetails.Futuristic && Model.ModelDetails.ModelId == CWConfiguration.MahindraAlturasModelid)
{
    <script type="text/javascript" src="/static/js/renaultform.js" defer></script>
}

@if(Model.ShowEmiCalculator)
{
    <script type="text/javascript" src="/static/m/js/utils/emi-slug.js" defer></script>
    <script type="text/javascript" src="/static/js/utils/scroll-lock.js" defer></script>
    <script type="text/javascript" src="/static/js/utils/popup.js" defer></script>
    { Html.RenderPartial("~/Views/Shared/m/_EmiCalculator.cshtml"); }
}
    
<script type="text/javascript" src="/static/js/sticky-ad.js" defer></script>