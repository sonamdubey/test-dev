<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<configSections>
		<section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net"/>
	</configSections>
	<connectionStrings>
		<add name="RabbitMQServerList" connectionString="172.16.0.20"/>
		<add name="ReadOnlyConnectionString" connectionString="SERVER=172.16.0.26;UID=cwadmin;PASSWORD=cwadmin;DATABASE=bikewaledev;Allow User Variables=true;Min Pool Size=10;Max Pool Size=300"/>
		<add name="MasterConnectionString" connectionString="SERVER=172.16.0.26;UID=cwadmin;PASSWORD=cwadmin;DATABASE=bikewaledev;Allow User Variables=true;Min Pool Size=10;Max Pool Size=300"/>
	</connectionStrings>
	<appSettings>
		<add key="ReadOnlyConnectionString" value="SERVER=172.16.0.26;UID=cwadmin;PASSWORD=cwadmin;DATABASE=bikewaledev;Allow User Variables=true;Min Pool Size=10;Max Pool Size=300"/>
		<add key="MasterConnectionString" value="SERVER=172.16.0.26;UID=cwadmin;PASSWORD=cwadmin;DATABASE=bikewaledev;Allow User Variables=true;Min Pool Size=10;Max Pool Size=300"/>
		<add key="ConsumerName" value="BWPQConsumer-Local"/>
		<add key="QueueName" value="BWPQProcessingQueue"/>
		<add key="RabbitMsgTTL" value="60000"/>
	</appSettings>
   
  <log4net>
  <appender name="InfoLog" type="log4net.Appender.RollingFileAppender">
    <file value="logs/LogInfo.txt"/>
    <appendToFile value="true"/>
    <lockingModel type="log4net.Appender.FileAppender+MinimalLock"/>
    <rollingStyle value="Size"/>
    <maxSizeRollBackups value="5"/>
    <maximumFileSize value="1MB"/>
    <staticLogFileName value="true"/>
    <filter type="log4net.Filter.LevelMatchFilter">
      <levelToMatch value="INFO" />
    </filter>
    <layout type="log4net.Layout.PatternLayout">
      <conversionPattern value="%date [%thread] %level %logger - %message%newline%exception%newline"/>
    </layout>
  </appender>
  <appender name="GelfUdpAppender" type="Gelf4net.Appender.GelfUdpAppender, Gelf4net">
    <remoteAddress value="172.16.1.39" />
    <remotePort value="22002" />
    <layout type="Gelf4net.Layout.GelfLayout, Gelf4net">
      <param name="AdditionalFields" value="app:BikeWalePriceQuoteConsumer,version:1.0,Environment:Production,Level:%level" />
      <param name="Facility" value="Bikewale PriceQuote Consumer Logs" />
      <param name="ConversionPattern" value="[%t] %c{1} - %m" />
    </layout>
  </appender>
  <root>
    <level value="INFO"/>
    <appender-ref ref="GelfUdpAppender"/>
  </root>
</log4net>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2"/></startup></configuration>
