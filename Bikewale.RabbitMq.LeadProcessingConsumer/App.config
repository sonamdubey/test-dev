    <add key="HondaGaddiAPIUrl" value="http://hondalms.gaadi.com/lms/externalApi/girnarLeadHMSIApi.php?params="/>
    <add key="BajajFinanceAPIUrl" value="http://agni.bajajauto.co.in:9072/icrmuat/index.php?r=CampaignAPI/3MLeadsToiCRM"/>
